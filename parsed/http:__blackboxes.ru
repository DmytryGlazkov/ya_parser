<script type="text/javascript">
	function Calc()
	{
		var box_size = $('select[name=box_size]').val();
		var time_count = $('select[name=time_count]').val();
		var time_type = $('select[name=time_type]').val();
	
		if(box_size && time_count && time_type)
		{
			$.ajax({
				url: "/bitrix/components/blackboxes/calc/templates/.default/ajax.php",
				data: "box_size=" + box_size + "&time_count=" + time_count + "&time_type=" + time_type,
				type: "POST",
				dataType: "json",
				cache: false,
				success: function(response){
					if(typeof(response) == 'object')
					{
						$("#calc_sum").html(response.s_sum);
					}
				}
			});
		}
	}
	$(document).ready(function(){
		Calc();
	});
	$(document).on("change","select[name=box_size]", function(){
		Calc();
	});
	$(document).on("change","select[name=time_count]", function(){
		Calc();
	})
	$(document).on("change","select[name=time_type]", function(){
		Calc();
	})
</script>
 
