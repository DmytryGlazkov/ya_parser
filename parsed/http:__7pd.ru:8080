<script>
$(document).ready(function(){

	function pre(v) { return $.trim(v.val()).replace(/<\/?[^>]+>/gi, '').replace(/[ ]+/gi, ' '); }

	$('form[name=callbackform]').submit(function() 
	{
	  	var uname = $(this).find(':input[name=uname]');
	  	var phone = $(this).find(':input[name=phone]');
	  	var city = $(this).find(':input[name=city]');
	  	var cap = $(this).find(':input[name=cap]');
	  	var q = $(this).find('select[name=city] option:selected');
	  	var error = 0;
	  	
	  	uname.attr('value', pre(uname));
	  	phone.attr('value', pre(phone));
	  	city.attr('value', pre(city));
	  	cap.attr('value', pre(cap));
	  		  	
	  	if( uname.val() == '' ) { uname.css('border-color', 'red'); error = 1; }
	  	else { uname.css('border-color', '#99A0AD');}
	  	
	  	if( phone.val() == '' || !phone.val().match(/[0-9]+/) ) { phone.css('border-color', 'red'); error = 1; }
	  	else { phone.css('border-color', '#99A0AD');}
	  	
	  	if( city.val() == '' ) { city.css('border-color', 'red'); error = 1; }
	  	else { city.css('border-color', '#99A0AD');}
	  	
	  	if( cap.val() != 11 ) { cap.css('border-color', 'red'); error = 1; }
	  	else { cap.css('border-color', '#99A0AD');}
	  	
	  	if( error ) {
	  		$('#ans').html('<b>������! ��������� ��� ���� �����</b>');
	  		setTimeout(	function(){ $('#ans').text(''); }, 2000);
	  	}
	  	else
	  	{
	  		//alert($(this).serialize());
	  		
	  		$.ajax({
   				url: '/send_call_back.php',             
			    dataType : 'text',       
			    type : 'post',
			    data: $(this).serialize(),
			    beforeSend: function (data, textStatus) { 
				    $('#ans').html("<img align='center' src='/images/ajax-loader.gif' border='0'>");
    			},
    			success: function (data, textStatus) { 
				    //$('#ans').html(data);
				    //setTimeout(	function(){ $('#ans').text(''); }, 10000);
				    $('#ans').text('');
				    $("div[id^='popwcont']").hide(0);
				    $('#popwcont_cbsuc').modal({overlayClose:true});
    			},
			});
	  	}
	  	
		return false;
	});
	
})
</script>

		</div>
	</div>
