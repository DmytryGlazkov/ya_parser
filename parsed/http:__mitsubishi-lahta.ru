        <script type="text/javascript">
      (function($) {
        $.fn.parallaxSlider = function(options) {
          var opts = $.extend({}, $.fn.parallaxSlider.defaults, options);
          return this.each(function() {
            var $pxs_container  = $(this),
            o         = $.meta ? $.extend({}, opts, $pxs_container.data()) : opts;
            
            //�������� �������
            var $pxs_slider   = $('.pxs_slider',$pxs_container),
            //�������� � ��������
            $elems      = $pxs_slider.children(),
            //����� ���������� ���������
            total_elems   = $elems.length,
            //������ ���������
            $pxs_next   = $('.pxs_next',$pxs_container),
            $pxs_prev   = $('.pxs_prev',$pxs_container),
            //������� �����������
            $pxs_bg1    = $('.pxs_bg1',$pxs_container),
            $pxs_bg2    = $('.pxs_bg2',$pxs_container),
            $pxs_bg3    = $('.pxs_bg3',$pxs_container),
            //������� �����������
            current     = 0,
            ///��� �������
            current_bg    = 0,
            //��������� ��������
            $pxs_thumbnails = $('.pxs_thumbnails',$pxs_container),
            //���������
            $thumbs     = $pxs_thumbnails.children(),
            //�������� ��� ������ ����������������
            slideshow,
            //�������� �����������
            $pxs_loading  = $('.pxs_loading',$pxs_container),
            $pxs_slider_wrapper = $('.pxs_slider_wrapper',$pxs_container);
              
            //������� ��������� ��� �����������
            var loaded    = 0,
            $images   = $pxs_slider_wrapper.find('img');
              
            $images.each(function(){
              var $img  = $(this);
              $('<img/>').load(function(){
                ++loaded;
                if(loaded == total_elems*2){
                  $pxs_loading.hide();
                  $pxs_slider_wrapper.show();
                    
                  //������ ����������� (������������, ��� ��� ����������� ����� ���������� �������)
                  var one_image_w   = $pxs_slider.find('img:first').width();
              
                  /*
                  ����� ���������� ������ ��������, ������� �� ��� ��������� 
                  � ������ ���������
                   */
                  setWidths($pxs_slider,
                  $elems,
                  total_elems,
                  $pxs_bg1,
                  $pxs_bg2,
                  $pxs_bg3,
                  one_image_w,
                  $pxs_next,
                  $pxs_prev);
              
                  /*
                    ������������� ������ ��������
                    � ������������ �� ����������
                  */ 
                  $pxs_thumbnails.css({
                    'width'     : 970 + 'px',
                    'margin-left'   : 0 + 'px'
                  });
                  var spaces  = one_image_w/(total_elems+1);
                  $thumbs.each(function(i){
                    var $this   = $(this);
                    var left  = /*spaces*(i+1) - $this.width()/2*/ 0;
                    $this.css('left',left+'px');
                      
                    if(o.thumbRotation){
                      var angle   = Math.floor(Math.random()*41)-20;
                      $this.css({
                        //'-moz-transform'  : 'rotate('+ angle +'deg)',//
                        //'-webkit-transform' : 'rotate('+ angle +'deg)',//
                        //'transform'     : 'rotate('+ angle +'deg)'//
                      });
                    }
                                                                         
                    /*��� ����������� ������� ��� ����������, ��� ��������������
                    $this.bind('mouseenter',function(){
                      $(this).stop().animate({top:'-10px'},100);
                    }).bind('mouseleave',function(){
                      $(this).stop().animate({top:'0px'},100);
                    });*/
                  });
                    
                  //������ ������ ��������� ���������
                  highlight($thumbs.eq(0));
                    
                  //�������������� ��� ������� ������ ���������
                  $pxs_next.bind('click',function(){
                    ++current;
                    if(current >= total_elems)
                      if(o.circular)
                        current = 0;
                    else{
                      --current;
                      return false;
                    }
                    --current_bg;
                    if(current_bg < 0)
                      if(o.circular)
                        current_bg = total_elems - 1;
                    else{
                      ++current_bg;
                      return false;
                    }
                    highlight($thumbs.eq(current));
                    slide(current,
                    $pxs_slider,
                    $pxs_bg3,
                    $pxs_bg2,
                    $pxs_bg1,
                    o.speed,
                    o.easing,
                    o.easingBg,current_bg);
                  });
                  
                  $pxs_prev.bind('click',function(){
                    --current;
                    if(current < 0)
                      if(o.circular)
                        current = total_elems - 1;
                    else{
                      ++current;
                      return false;
                    }
                      ++current_bg;
                    if(current_bg >= total_elems)
                      if(o.circular)
                        current_bg = 0;
                    else{
                      --current_bg;
                      return false;
                    }
                    highlight($thumbs.eq(current));
                    slide(current,
                    $pxs_slider,
                    $pxs_bg3,
                    $pxs_bg2,
                    $pxs_bg1,
                    o.speed,
                    o.easing,
                    o.easingBg,current_bg);
                  });
              
                  /*
                   ������� �� ��������� �������� � �������������� ���������������� �����������
                   */

                  $thumbs.bind('mouseenter',function(){
                    $(this).addClass("hasFocus");
                    var h = $(this);
                    setTimeout(function(){ 
                            if (h.hasClass("hasFocus")) {
                              // var $thumb = $(this);
                          // highlight($thumb);
                          highlight(h);
                          //���� �������� ����������������, ��������� ��� ��� ������� ������ ����
                          if(o.auto)
                            clearInterval(slideshow);
                          //���������� �����
                          var old_current_bg=current_bg;
                          //���������� ���
                          var old_current = current;

                          // current_bg   = $thumb.index();
                          current_bg  = h.index();
                          current=old_current_bg;

                          if(current>7) current=0;
                          if(current<0) current=7;
                          slide(current,
                          $pxs_slider,
                          $pxs_bg3,
                          $pxs_bg2,
                          $pxs_bg1,
                          o.speed,
                          o.easing,
                          o.easingBg,current_bg);

                          $('.slider').hide();
                            $('.pxs_bg').show();
                            $('.pxs_slider').show();
                            }
                    }, 1000 );
                    
                  });

                  $thumbs.bind('mouseleave',function(){
                    $(this).removeClass("hasFocus");

                      $('.slider').show();
                      $('.pxs_bg').hide();
                      $('.pxs_slider').hide();
                  });
              
                
              
                  /*
                  ���������� ����������������, 
                  ���� ����������� ��������������� �����
                   */
                  if(o.auto != 0){
                    o.circular  = true;
                    slideshow = setInterval(function(){
                      $pxs_next.trigger('click');
                    },o.auto);
                  }
              
                  /*
                  ��� ��������� �������� ����
                  ����� ����������� ������ ���������
                  �� ������ ����� ������ ����.
                  ����� ����� ����������� ������� �������,
                  ����� ������������ ����� �����.
                   */
                  $(window).resize(function(){
                    w_w = $(window).width();
                    setWidths($pxs_slider,$elems,total_elems,$pxs_bg1,$pxs_bg2,$pxs_bg3,one_image_w,$pxs_next,$pxs_prev);
                    slide(current,
                    $pxs_slider,
                    $pxs_bg3,
                    $pxs_bg2,
                    $pxs_bg1,
                    1,
                    o.easing,
                    o.easingBg,current_bg);
                  });

                }
              }).error(function(){
                alert('here')
              }).attr('src',$img.attr('src'));
            });
              
              
              
          });
        };
        
        //������� ������ ����
        var w_w       = $(window).width();
        
        var slide     = function(current,
        $pxs_slider,
        $pxs_bg3,
        $pxs_bg2,
        $pxs_bg1,
        speed,
        easing,
        easingBg,current_bg){
          var slide_to  = parseInt(-w_w * current);
          var slide_to_minus  = parseInt(-w_w * current_bg);
          
          var v = $('#pxs_container').width();
          v=($(window).width()-v)/2;
          slide_to_minus=slide_to_minus-v;
          if(current_bg==0) slide_to_minus = slide_to_minus+25;
          $pxs_slider.stop().animate({
            left  : slide_to_minus + 'px'
          },speed, easing);
          $pxs_bg3.stop().animate({
            left  : slide_to/2 + 'px'
          },speed, easingBg);
          $pxs_bg2.stop().animate({
            left  : slide_to/4 + 'px'
          },speed, easingBg);
          $pxs_bg1.stop().animate({
            left  : slide_to/8 + 'px'
          },speed, easingBg);
        }
        
        var highlight   = function($elem){
          $elem.siblings().removeClass('selected');
          $elem.addClass('selected');
        }
        
        var setWidths   = function($pxs_slider,
        $elems,
        total_elems,
        $pxs_bg1,
        $pxs_bg2,
        $pxs_bg3,
        one_image_w,
        $pxs_next,
        $pxs_prev){
          /*
          ������ �������� - ��� ������ ���� ���������� �� ����� ���������� ��������� � ��������
           */
          var pxs_slider_w  = w_w * total_elems;
          $pxs_slider.width(pxs_slider_w + 'px');
          //������ ������� ����� ������ ������ ������ ����
          $elems.width(w_w + 'px');
          /*
          �� ����� ������������� ������ ������� ����������� ����.
          �������� ����� ����������� � ������� pxs_slider
           */
          $pxs_bg1.width(pxs_slider_w + 'px');
          $pxs_bg2.width(pxs_slider_w + 'px');
          $pxs_bg3.width(pxs_slider_w + 'px');
          
          /*
          ��� ����� ������������� ������:
          windowWidth/2 - imgWidth/2 + ��������� ���� (����� �� �������� ����� �����������)
           */
          var position_nav  = w_w/2 - one_image_w + 85;
          $pxs_next.css('right', position_nav + 'px');
          $pxs_prev.css('left', position_nav + 'px');
        }
        
        $.fn.parallaxSlider.defaults = {
          auto      : 0,  //�������� � �������� ��� �������������� ������������� ��������,
                      //���� �������� 0 - ������������� ���������
          speed     : 1000,//�������� ��������������� ������
          easing      : 'jswing',//������ �������� ��� ��������
          easingBg    : 'jswing',//������ �������� ��� �������� ����
          circular    : true,//��������������� �� �����
          thumbRotation : true//��������� �������������� ��������� �������
        };
        //easeInOutExpo,easeInBack
      })(jQuery);
    </script>

