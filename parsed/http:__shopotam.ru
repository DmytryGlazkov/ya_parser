<script>

    $(function() {
        var lazyBlocks =  $('.lazy');
        var offset = 200;

        var loadNextBlock = function() {
            if(!lazyBlocks.length) {
                return;
            }
            var container = lazyBlocks.first();
            if(container.data('status') == 'loading') {
                return;
            }
            var objectBottomPosition = container.position().top + container.outerHeight();
            var windowBottomPosition = $(window).scrollTop() + $(window).height();
            if(windowBottomPosition + offset > objectBottomPosition) {
                container.data('status', 'loading');
                $.ajax({
                    url: container.data('url'),
                    dataType: 'html',
                    success : function(response, status) {
                        lazyBlocks = lazyBlocks.slice(1);
                        container.html(response);
                        loadNextBlock();
                    }
                });
            }
        };

        $(window).scroll(function() {
            loadNextBlock();
        });
        loadNextBlock();
    });

</script>

