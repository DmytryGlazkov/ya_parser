$(function(){
if ($("#carousel_big_ban").length) {
	$("#carousel_big_ban").jcarousel({
		scroll: 1,
		auto: $('.carousel_big_ban_wrap').attr('data-timeshow')/1000,
		wrap: 'circular',
		initCallback: control_bigban,
		itemFirstInCallback: control_scroll_bigban,
		buttonNextHTML: null,
		buttonPrevHTML: null
	});
}
