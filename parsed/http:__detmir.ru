function control_bigban(carousel) {
	$('.carousel_big_ban_wrap .c_dots li').bind('click', function() {
		$('.carousel_big_ban_wrap .c_dots li').removeClass('current');
		$(this).addClass('current');
		carousel.scroll($.jcarousel.intval($(this).val()));
	});
	$('.carousel_big_ban_wrap .next').bind('click', function() { carousel.next();});
	$('.carousel_big_ban_wrap .prev').bind('click', function() { carousel.prev();});
}
function control_scroll_bigban(carousel, liElement, intemIndex, action) {
	$('.carousel_big_ban_wrap .c_dots li').removeClass('current');
	var controlIndex = (intemIndex-1) % $('.carousel_big_ban_wrap .c_dots li').length;
	$('.carousel_big_ban_wrap .c_dots li').eq(controlIndex).addClass('current');
}
