	y_experement = "http://semena.hozvo.ru/index.php?route=information/information&information_id=12"
	//alert(y_experement.indexOf(window.location.pathname));
	
	if (!route) {
		if (window.location.pathname == "/") {
			$('#tab_home').addClass('selected');
		} else {
			if (y_experement.indexOf(window.location.pathname) > 0) {
				$('#tab_experement').addClass('selected');
			} else {
				$('#tab_home').addClass('selected');
			}
		}
	} else {
		part = route.split('/');
		
		if (route == 'common/home') {
			$('#tab_home').addClass('selected');
		} else if (route == 'account/login') {
			$('#tab_login').addClass('selected');	
		} else if (part[0] == 'account') {
			$('#tab_account').addClass('selected');
		} else if (route == 'checkout/cart') {
			$('#tab_cart').addClass('selected');
		} else if (part[0] == 'checkout') {
			$('#tab_checkout').addClass('selected');
		} else {
			$('#tab_home').addClass('selected');
		}
	}
});
//--></script>
