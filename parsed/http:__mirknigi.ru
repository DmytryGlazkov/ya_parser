                color: '#fff',
                loadSpeed: 200,
                opacity: 0.0
            },
            effect: 'slideEffect',
            closeOnClick: true
        });

        $(".errorModal").overlay({
            top: windowTop,
            oneInstance: false,
            mask: {
                color: '#fff',
                loadSpeed: 200,
                opacity: 0.0
            },
            effect: 'slideEffect',
            closeOnClick: true
        });

        // adds "slideEffect" to the overlay
        $.tools.overlay.addEffect("slideEffect", function(position, done) {
                    this.getOverlay().css(position).show("slide", {direction: 'down'}, 500);
                },
                function(done) {
                    this.getOverlay().hide("slide", {direction: 'down'}, 500);
                }
        );

        $( ".btn-close-right" ).mouseover(function() {
            $(this).addClass("btn-close-right-red ");
        });

        $( ".btn-close-right" ).mouseout(function() {
            $(this).removeClass("btn-close-right-red ");
        });

        //form validation
        var smallNewsletterFormJS = new VarienForm('newsletter-validate-detail');
        $(smallNewsletterFormJS.form).on('submit', function (event) {
            if (smallNewsletterFormJS.validator && smallNewsletterFormJS.validator.validate() && $(this).attr("isAllowSubmit") != 0) {

                $(this).attr("isAllowSubmit", 0);

                // Add to oracle
                var firstname = $('.firstname').val();
                var lastname = $('.lastname').val();
                var email = $('.email').val();

                $.ajaxSetup({
                    timeout: 10000
                });

                $.ajax({
                    url: 'http://moymir.ru/oracleconnect/index/addcustomer/',
                    type: 'POST',
                    dataType: "json",
                    contentType: "application/x-www-form-urlencoded; charset=UTF-8",
                    data: {'first':firstname, 'last':lastname, 'email':email}
                }).done(function(json) {

                    if (json.result == 'done'){
                        $('#username-success-window').text(firstname + ',');
                        $(".successModal").overlay().load();
                    }
                    else if (json.result == 'done:book'){
                        $('#username-success-book-window').text(firstname + ',');
                        $(".successBookModal").overlay().load();
                    }
                    else if (json.result == 'done:2opt_in'){
                        $('#username-success-2optIn-window').text(firstname + ',');
                        $(".success2optInModal").overlay().load();
                    }
                    else if (json.result == 'fail:emailexist'){
                        $('#username-exist-window').text(firstname + '!');
                        $(".existEmailModal").overlay().load();
                    }
                    else if (json.result == 'fail:emailnotvalid'){
                        $('#username-not-valid-window').text(firstname + ',');
                        $(".notValidEmailModal").overlay().load();
                    }
                    else if (json.result == 'fail:emailnull'){
                        $('#not-valid-fields').text('e-mail');
                        $(".validationModal").overlay().load();
                    }
                    else if (json.result == 'fail:firstnull'){
                        $('#not-valid-fields').text('Имя');
                        $(".validationModal").overlay().load();
                    }
                    else if (json.result == 'fail:lastnull'){
                        $('#not-valid-fields').text('Фамилия');
                        $(".validationModal").overlay().load();
                    }
                    else if (json.result == 'fail:error'){
                        $(".errorModal").overlay().load();
                    }
                    $("#newsletter-validate-detail").attr("isAllowSubmit", 1);
                })
                .fail(function(){
                    $(".errorModal").overlay().load();
                    $("#newsletter-validate-detail").attr("isAllowSubmit", 1);
                });

                // GA tracking
                try {
                    _gaq.push(['_trackEvent', 'Newsletter', 'RegNewsletter']);
                } catch (err) {}
                // YM tracking
                try {
                    window.yaCounter18548734.reachGoal('RegNewsletter');
                } catch (err) {}
                return false;
            } else {
                event.preventDefault();
                return false;
            }
        });
    });
