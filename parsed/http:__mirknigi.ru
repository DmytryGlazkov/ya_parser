                    if (data.error == true) {
                        $('#preloader').hide();
                        $('#submit-callback').show();
                        alert('Ошибка. Пожалуйста, свяжитесь с нами или повторите попытку позже.');
                    } else {
                        $('#preloader').hide();
                        $('#callback').hide();
                        $('#callback-modal').hide();
                        $('#callback-message').show();
                    }
                },
                error: function () {
                    alert('Ошибка. Пожалуйста, свяжитесь с нами или повторите попытку позже.');
                    $('#preloader').hide();
                    $('#submit-callback').show();
                }
            });
        });

    });
    //]]>
</script>
                    </div>
    </body>
</html>