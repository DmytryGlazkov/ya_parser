MLP.MainPage = {
	current_page: 0,
	
	init_autocomplite_items: function(){
		$('.autocomplite_item').click(function(){
			$('#main_seach').val($(this).html());
			MLP.MainPage.update_seach_results($('#main_seach').val());
		});
	},
	
	activate_seach: function(elem){
		if ($(elem).hasClass('no_input')) {
			$(elem).val('');
		}
		$(elem).removeClass('no_input');
	},
	
	update_seach_results: function(val){
		if ('' != val) {
			$('#middle_content').removeClass('text_included');
			$('#categoryes_seach, #category_content, #categoryes_content').hide();
			$('#seach_content').show();
			$('#seach_content').html('');
			var result = new Array();
			for (var i = 0; i < seach_array.length; i++) {
				var seach_val = seach_array[i]['val'].toUpperCase();
				var pos = seach_val.indexOf(val.toUpperCase());
				if (-1 != pos) {
					seach_array[i]['pos'] = pos;
					result[result.length] = seach_array[i];
				}
			}
			if (0 != result.length) {
				$('#autocomplite_content').html('');
				if (1 == result.length && val.toUpperCase() == result[0]['val'].toUpperCase()) {
					$('#autocomplite_content').hide();
				}
				else {
					$('#autocomplite_content').show();
				}
				for (var i = 0; i < result.length; i++) {
					var first_string = result[i]['val'].substring( 0, result[i]['pos'] );
