	show_category_clear_page: function(category, page){
		$('#category_content').show();
		var total_pages = Math.ceil( clean_sites_array[category].split(',').length / items_on_page );
		
		var start_item_index = (page - 1) * items_on_page;
		var end_item_indes = (start_item_index + items_on_page) < clean_sites_array[category].split(',').length ? (start_item_index + items_on_page) : clean_sites_array[category].split(',').length;
		var clear_sites = clean_sites_array[category].split(',');
		if ( $('#category' + category + '_p' + page).html() == '' ){
			for (var i =  start_item_index ; i < end_item_indes; i++) {
				$('#category' + category + '_p' + page).append( MLP.MainPage.site_clear_preview(clear_sites[i]));
			}	
		}
		$('#category' + category + '_p' + page).addClass('full_page');
		
		var delta = $('#category' + category + '_p' + page).parent().offset().left - $('#category' + category + '_p' + page).offset().left;
		$('#category_content_wrapper').animate( {'left': delta+'px'} );
		
		if ( total_pages > 1 ){
			$('#paging_block').html('');
			
			var paging = '<a href="javascript: void(0)" onclick="MLP.MainPage.show_category_clear_page( ' + category + ' , ' + ( page == 1 ? 1 : page - 1)  + ' );">&lt;</a> ' ;
			for ( var i = 1; i <= total_pages; i++ ){
				paging = paging + '<a href="javascript: void(0)" class="' + ( i == page ? 'active' : '' ) + '" onclick="MLP.MainPage.show_category_clear_page( ' + category + ' , ' + i + ' );">' + i + '</a> ' ;
			}
		 	paging = paging + '<a href="javascript: void(0)" onclick="MLP.MainPage.show_category_clear_page( ' + category + ' , ' + ( page == total_pages ? total_pages : page + 1)  + ' );">&gt;</a> ' ;
		
			$('#paging_block').html(paging);	
		}
		MLP.MainPage.set_url_category(category, page);		
		MLP.MainPage.set_url_category_link();
		MLP.MainPage.init_site_menu();
		MLP.MainPage.lazy_load_action();
	},
	
	show_category_page: function(category, page){
