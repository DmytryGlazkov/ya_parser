		if ( total_pages > 1 ){
			$('#paging_block').html('');
			
			var paging = '<a href="javascript: void(0)" onclick="MLP.MainPage.show_category_page( ' + category + ' , ' + ( page == 1 ? 1 : parseInt(page) - 1)  + ' );">&lt; назад</a> ' ;
