	activeType : '',

	set_url_category_link: function( category, page ){
		var allClass = (MLP.MainPage.activeType == 'all' || MLP.MainPage.activeType == '') ? "class='cat_menu active'" : "class='cat_menu'";
		var visitkaClass = (MLP.MainPage.activeType == 'visitka') ? "class='cat_menu active'" : "class='cat_menu'";
		var shopClass = (MLP.MainPage.activeType == 'shop') ? "class='cat_menu active'" : "class='cat_menu'";
		
		html = '<div ' + allClass + ' id="cat_all" href="javascript:;" onclick="MLP.MainPage.show_cat_only(\'all\')">Все категории</div>';
		html += '<div ' + visitkaClass + ' id="cat_visitka" href="javascript:;" onclick="MLP.MainPage.show_cat_only(\'visitka\')">Сайт визитка</div>';
		html += '<div ' + shopClass + ' id="cat_shop" href="javascript:;" onclick="MLP.MainPage.show_cat_only(\'shop\')">Интернет магазин</div>'
		$('#category_types').html(html);
	},
	
	show_cat_only: function (cat, switch_to_category) {
		href = "?type=" + cat + "&ajax=1" + current_hash;
		
		MLP.MainPage.activeType = cat;
		
		$('.active').removeClass('active');
		$('#cat_' + cat).addClass('active');
		
		$.get(href, function(data) {
			if ($('#step2_title').length == 0) {
				if ( $('#foter_arrow') ){
					$('#foter_arrow').parent().remove();
				}
				if ( $('#social_widget_html') ){
					$('#social_widget_html').remove();
				}
				$('#total').replaceWith(data);
				if ($('div#footer').length > 1) {
					$($('div#footer')[0]).remove();
				}
			} else {
				$('.seach_wrapper').replaceWith(data);
			}
			MLP.MainPage.init_last_page();
			if ( switch_to_category ){
			    MLP.MainPage.show_category(1);
			}
		});
	},
	
	init_last_page: function(){
		var hash = window.location.hash;
		hash = hash.substr(1);
		var entries = hash.split('&');
		var page = undefined;
		var category = undefined;
		var seach = undefined;
		for ( i = 0; i < entries.length; i++ ){
		    var entry = entries[i].split('=');
		    if ( 'page' == entry[0] && undefined != entry[1] ){
		        page = entry[1];
		    }
		    if ( 'category' == entry[0] && undefined != entry[1] ){
		        category = entry[1];
		    }
		    if ( 'seach' == entry[0] && undefined != entry[1] ){
		        seach = entry[1];
		    }
		}
		if ( category != undefined && page != undefined ){
			$('#middle_content').removeClass('text_included');
		    MLP.MainPage.show_category( category );
		    MLP.MainPage.show_category_page( category, page);
		} else {
			$('#middle_content').addClass('text_included');
			MLP.MainPage.update_seach_results('');
		} 	
	},
	
	test_hash: function (){
	    if ( current_hash != window.location.hash && !dont_test_hash ){
	        current_hash = window.location.hash;
	        MLP.MainPage.init_last_page();
	    }
   		setTimeout(  MLP.MainPage.test_hash, 100);
    },
	
	change_site_view: function (ind) {
		$('.site_view').hide();
		$('.site_view' + ind).show();
		if (ind == 1) {
			$('.category_seach_clear_item').removeClass('active_category');
			MLP.MainPage.show_category(MLP.MainPage.selected_category, true);
		} else {
			$('.category_seach_item').removeClass('active_category');
			MLP.MainPage.show_category_clear(MLP.MainPage.selected_category_clear);			
		}
	}
