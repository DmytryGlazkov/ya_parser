function send_site_request() {
	var site = $('#send_site_name').val();
	var phone = $('#send_site_phone').val();
	var email = $('#send_site_email').val();
	
	$('.send_site_request .send_site_field.error').removeClass('error');
	
	if (site.length == 0) {
		$('#send_site_name').addClass('error');
		$('#send_site_name').focus();
		return;
	}
	
	if (phone.length == 0) {
		$('#send_site_phone').addClass('error');
		$('#send_site_phone').focus();
		return;
	}
	
	if (email.length == 0) {
		$('#send_site_email').addClass('error');
		$('#send_site_email').focus();
		return;
	}
	
	$.post(
		'about?send_site_request',
		{	
			'site' : site,
			'phone' : phone,
			'email' : email
		},
		function(data) {
			$('#send_site_tip div').html(data);
			$('#send_site_tip').show();
			$('.send_site_request').height(620);
			setTimeout("hide_send_site_window()", 2000);
		}
	);
}
