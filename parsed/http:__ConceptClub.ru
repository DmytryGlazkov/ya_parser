function send_newsletter() {
	var email = $('#newsletter_email').val()
		, placeholder = $('#newsletter_content')
		, message = placeholder.find('.message').first()
		, button = placeholder.find('button').first()
		;

	button.attr('disabled', 'disabled');

	$.ajax({
		type: "GET",
		url: 'http://www.conceptclub.ru/main/sendnewsletter',
		data: 'email=' + email,
		dataType: "json",
		success: function (result) {
			button.removeAttr('disabled');
			if (result.success) {
				if (message.hasClass('has-error')) {
					message.removeClass('has-error').css('visibility', 'hidden');
				}

				$('#subscribe-popup-promo-code').text(result.promo_code);
				$('#a-subscribe-popup').trigger('click');
			} else {
				message
					.addClass('has-error')
					.text(result.message)
					.css('visibility', 'visible')
				;
			}
		}
	});
}

function subscribe_popup()
{
	var email = $('#subscribe-popup-form_email').val()
		, message = $('#subscribe-popup-form').find('.message').first()
		, $button = $('#subscribe')
		;

	$button.data('default-text', $button.text());
	$button.text('подождите..').attr('disabled', 'disabled');

	$.ajax({
		type: "GET",
		url: 'http://www.conceptclub.ru/main/sendnewsletter',
		data: 'email=' + email,
		dataType: "json",
		success: function (result) {
			$button
				.text($button.data('default-text'))
				.removeAttr('disabled')
			;
			if (result.success) {
				if (message.hasClass('has-error')) {
					message.removeClass('has-error').css('visibility', 'hidden');
				}

				$('#subscribe-popup-promo-code').text(result.promo_code);
				$.fancybox.close();

				$('#a-subscribe-popup').trigger('click');
			} else {
				message
					.addClass('has-error')
					.text(result.message)
					.css('visibility', 'visible')
				;
			}
		}
	});
}
</script>
