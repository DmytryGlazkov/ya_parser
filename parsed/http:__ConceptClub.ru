//send
$("#call_send_b").click(function(){
var err_text="заполните поле";
var name=$("#call_u_name").val();
var tel=$("#call_u_tel").val();
var why=$("#call_why option:selected").html();
if(why.search("Другое")!=-1){
var why_add=$("#call_why_add").val();
}else{
var why_add="";
}
err=0;
if(name==""){
	err++
	$("#call_u_name").css("border","1px solid red");
	if(!$("#call_u_name").prev().hasClass("err")){
	$("<span />").addClass("err").text(err_text).insertBefore($("#call_u_name"));
	}
}else{
$("#call_u_name").css("border","");
if($("#call_u_name").prev().hasClass("err")){
	$("#call_u_name").prev().remove();
}
}

if(tel==""){
	err++;
	$("#call_u_tel").css("border","1px solid red");
	if(!$("#call_u_tel").prev().hasClass("err")){
	$("<span />").addClass("err").text(err_text).insertBefore($("#call_u_tel"));
	}
}else{
$("#call_u_tel").css("border","");
if($("#call_u_tel").prev().hasClass("err")){
	$("#call_u_tel").prev().remove();
}
}

if($("#call_why").parent().children("div").hasClass("inactive")){
	err++;
	$("#call_why").parent().children("div").css("border","1px solid red");
	if(!$("#call_why").parent().prev().hasClass("err")){
	$("<span />").addClass("err").text(err_text).insertBefore($("#call_why").parent());
	}
}else{
$("#call_why").parent().children("div").css("border","");
if($("#call_why").parent().prev().hasClass("err")){
	$("#call_why").parent().prev().remove();
}
}

if(why.search("Другое")!=-1 && why_add==""){
err++;
$("#call_why_add").css("border","1px solid red");
if(!$("#call_why_add").prev().hasClass("err")){
$("<span />").addClass("err").text(err_text).insertBefore($("#call_why_add"));
}
}else{
$("#call_why_add").css("border","");
if($("#call_why_add").prev().hasClass("err")){
	$("#call_why_add").prev().remove();
}
}

if(err!=0){
	return false;
}

$.post("/return_good/send_call_back",{
'name':name,
'tel':tel,
'why':why,
'why_add':why_add
},function(rez){
if(rez=='ok'){
$("<span />").attr("id","call_em_ok").css("color","#2D5D36").text('Спасибо! Ваша заявка отправлена.').insertBefore($("#call_send_b"));
setTimeout(function(){
$("#call_back_pop .ret_close_b").click();
},3000);

}else{
alert("Произошла ошибка.");
}

	});
	
	
	
	return false;
});
	
});

