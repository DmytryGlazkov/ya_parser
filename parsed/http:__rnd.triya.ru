						$next.addClass('next');
					else
						$('.banner:first', $photo).addClass('next');

					set();

					timer = window.setTimeout(self.next, interval);
				}

			}

			$('.banner', $photo).click(function (){
				var $this = $(this);
				if ($this.hasClass('next'))
				{
					self.next();
					return false;
				}
				else if ($this.hasClass('prev'))
				{
					self.prev();
					return false;
				}
			}).hover
			(
				function()
				{
					window.clearTimeout(timer);
				},
				function()
				{
					window.clearTimeout(timer);
					timer = window.setTimeout(self.next, 1700);
				}
			);

			set();
			timer = window.setTimeout(self.next, interval);
