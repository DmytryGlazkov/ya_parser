	<script type="text/javascript">
	$(document).ready(function(){

		function rebuild () {
			var $this = $(this),
				menuHeight = $this.height(),
				$columns = $('ul.root-item', this),
				height = 0,
				columns = [document.createElement('ul')],
				columnN = 0;
			columns[columnN].className = 'root-item';
			$columns.find('li').each(function () {
				var $this = $(this), newColumn;
				height += $this.outerHeight(true);
				if (height > menuHeight) {
					height = 0;
					columns[++columnN] = document.createElement('ul'),
					columns[columnN].className = 'root-item';
				}
				$this.appendTo(columns[columnN]);
			});
			$('ul.root-item', this).remove();
			for (var i = 0, c = columns.length; i < c; i++)
				this.appendChild(columns[i]);
			$this.addClass('rebuilt');
		}

		var timer;
		$catalogMenu = $('.catalog-menu.main');
		$catalogMenu.menuAim({
			activate: function(row) {
				$(row).addClass('open');
				$('.submenu:not(.rebuilt)', row).each(rebuild);
			},
			deactivate: function(row) {
				$(row).removeClass('open');
			},
			enter: function () {
				$catalogMenu.addClass('openSubmenu');
				window.clearTimeout(timer);
			},
			exitMenu: function(menu) {
				timer = window.setTimeout(function () {
					$catalogMenu.removeClass('openSubmenu');
					$('> li.open', menu).removeClass('open');
				}, 500);
			},
			rowSelector: '> li',
			submenuDirection: 'right'
		});
	});
</script>

