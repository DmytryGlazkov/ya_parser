	<script type="text/javascript">

		var Slider = function (){

			var self = this,
				$photo = $("#photo-frame .banners"),
				$control = $("#slider-buttons"),
				width = $photo.children('.banner:first').outerWidth(true),
				timer = 0,
				interval = 5000,
				speed = 300;

				self.$current = $(".current", $photo);
				self.length = $photo.children('.banner').length;

			set = function ()
			{
				var index = self.$current.index();
				$('.banner', $photo).each(function(i){
					var	$this = $(this),
						half = Math.ceil(self.length/2 + .49),
						left = $this.position().left,
						newLeft = ((i - index) - Math.floor(0.49 + (i - index) / (half + 1)) * self.length) * width;

					$this.css({
						'left': newLeft,
						'visibility': ((Math.abs(left) <= (width * 2)) || (Math.abs(newLeft) <= width)) ? 'visible' : 'hidden'
					});
				});

			}

			self.next = function()
			{
				window.clearTimeout(timer);

				var $next = $('div.selected', $control).removeClass('selected').next();
				if ($next.length > 0 )
					$next.addClass('selected');
				else
					$('div:first', $control).addClass('selected');

				$('.banner.prev', $photo).removeClass('prev');
				$('.banner.current', $photo).removeClass('current').addClass('prev');
				self.$current = $('.banner.next', $photo).removeClass('next').addClass('current');
				$next = self.$current.next('.banner');
				if ($next.length > 0 )
					$next.addClass('next');
				else
					$('.banner:first', $photo).addClass('next');

				set();

				timer = window.setTimeout(self.next, interval);
			}

			self.prev = function ()
			{
				window.clearTimeout(timer);

				var $prev = $('div.selected', $control).removeClass('selected').prev();
				if ($prev.length > 0 )
					$prev.addClass('selected');
				else
					$('div:last', $control).addClass('selected');

				$('.banner.next', $photo).removeClass('next');
				$('.banner.current', $photo).removeClass('current').addClass('next');
				self.$current = $('.banner.prev', $photo).removeClass('prev').addClass('current');
				$prev = self.$current.prev('.banner');
				if ($prev.length > 0 )
					$prev.addClass('prev');
				else
					$('.banner:last', $photo).addClass('prev');

				set();

				timer = window.setTimeout(self.next, interval);
			}

			self.goTo = function (id)
			{
				if (self.$current.attr('id') != id)
				{
					window.clearTimeout(timer);

					$('div.selected', $control).removeClass('selected');
					$('#slider-btn-' + id, $control).addClass('selected');

					$('.banner.prev, .banner.current, .banner.next', $photo).removeClass('prev current next');

					var id = 'banner-' + id;

					self.$current = $('#' + id, $photo).addClass('current');

					var $prev = self.$current.prev('.banner');
