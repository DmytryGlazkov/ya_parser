            (function(){
                var el   = document.getElementById('overlay');
                var dontshow = false;
                var now;
                if (!navigator.cookieEnabled) return;
                try {
                    var ls = window.localStorage;
                    ls.setItem('tryLocalStorage', '1');
                    ls.removeItem('tryLocalStorage');

                    if(
                        ls.getItem('overlay.firstShowDate')       ||
                        ls.getItem('overlay.overlayViewsCount')   ||
                        ls.getItem('overlay.overlayShowsCount')   ||
                        ls.getItem('overlay.firstShowTimestamp')
                    ){
                        // Keep hidden for three monthes
                        now = new Date;
                        now.setTime(now.getTime() + 123 * 86400000);
                        document.cookie = ['overlay_exit=1;expires=', now.toGMTString()].join('') + '; path=/;';

                        // And remove show count cookie
                        document.cookie = 'overlay_showcount=;expires=Thu, 01 Jan 1970 00:00:01 GMT; path=/';
                        dontshow = true;
                    };

                    /* Remove old keys */
                    ls.removeItem('overlay.firstShowDate');
                    ls.removeItem('overlay.overlayViewsCount');
                    ls.removeItem('overlay.overlayShowsCount');
                    ls.removeItem('overlay.firstShowTimestamp');
                } catch(e) {
                    dontshow = true;
                };


                if(
                    // Mobile and small screens
                    ('ontouchstart' in document.documentElement && window.screen.availWidth < 1024)
