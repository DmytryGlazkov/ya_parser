                ){
                    dontshow = true;
                } else {
                    if( /overlay_showcount=(\d+)/.test(document.cookie) ){
                        // Keep hidden for three monthes
                        now = new Date;
                        now.setTime(now.getTime() + 123 * 86400000);
                        document.cookie = ['overlay_exit=1;expires=', now.toGMTString()].join('') + '; path=/;';

                        // And remove show count cookie
                        document.cookie = 'overlay_showcount=;expires=Thu, 01 Jan 1970 00:00:01 GMT; path=/;';
                    } else {
                        // Keep hidden for three days
                        now = new Date;
                        now.setTime(now.getTime() + 3 * 86400000);
                        document.cookie = ['overlay_exit=1;expires=', now.toGMTString()].join('') + '; path=/;';

                        // And store count
                        now.setTime(now.getTime() + 120 * 86400000);
                        document.cookie = ['overlay_showcount=1;expires=', now.toGMTString()].join('') + '; path=/;';
                    };
                };
                if (!dontshow) {
                    el.style.display = 'block'; // Проверка на display == 'block' используется в overlay.js
