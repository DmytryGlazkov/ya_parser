            $.cookie( 'ya_map_city', cur_city, { path: '/', 'expires': 365, domain: '.tdpoisk.ru' } );
            get_current_city_value()
    	} );
        function get_current_city_value() {
        if( AllCities[cur_city] == undefined ) {
            cur_city = 'Ростов-на-Дону';
        }
        var cur_host = window.location.hostname.substr(0, window.location.hostname.indexOf('.'));
        var is_cur_host = false;
        for( id in AllCities ) {
            if( cur_host == AllCities[id]['code'] ) {
                is_cur_host = true;
                break;
            }
        }
        if( is_cur_host ) {
            $( '#current_city' ).attr( 'href', window.location.href.replace( cur_host + '.', AllCities[cur_city]['code'] + '.' ) );
        } else {
            $( '#current_city' ).attr( 'href', window.location.href.replace( '://', '://' + AllCities[cur_city]['code'] + '.' ) );
        }
        $( '#current_city' ).html( cur_city + ( AllCities[cur_city]['inet'] ? ' <ins class="b-ico b-ico_content_cart"></ins>' : '' ) );
        $( '#current_city' ).click( function() { selectCity( AllCities[cur_city]['id'] ) } );
        current_city = AllCities[cur_city]['id'];
        //$('#close_popup').click(function(){selectCity(AllCities[cur_city]['id']); $('#cities_popup' ).hide(); return false;});
    }
	function city_popup_close() {
        if (!$.cookie('user_city')) {
            selectCity(current_city);
		}
        $('#cities_popup' ).fadeOut();
		return false;
	}
	function selectCity(id) {
		$.cookie('user_city', id, {path: '/', 'expires': 365, domain: '.tdpoisk.ru'});
		return true;
	}
    $( document ).ready( function() {
