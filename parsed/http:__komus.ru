        <script type="text/javascript">
$(document).ready(function () {


/* ВЫПАДАЮЩИЕ ПАНЕЛИ "ВОЙТИ" И "ЛИЧНЫЙ КАБИНЕТ" */

	/*
	блоку t24_js_bottom
	класс t24_js_activ_left дает раскрытую левую панель "личный кабинет,
	класс t24_js_activ_right  дает раскрытую левую панель "войти"

	при наведении или уведении мыши с блоков t24_js_cabinet или t24_js_vhod  начинает работать таймер,
	когда значение таймера превышает значение predel_zhdemms_bp,
	то раскрывается или закрывается соответствующая панель
	*/


	/* КОНСТАНТЫ И ПЕРЕМЕННЫЕ */
	//количество времени, которое прошло с начала отсчета таймера
	var zhdemms_bp = 0;
	//метка времени в момент начала отсчета таймера
	var start_time_ph;
	//метка текущего времени
	var current_time_bp;
	//вспомогательные переменные для получения меток времени
	var now_time_bp;
	//переменные, которые говорят о том, должен ли идти таймер если 1, то нет, если 0, то да
	var timer_stoop_bp = 1;
	//время ожидания, после которого показывается/убирается выпадающая панель
	var predel_zhdemms_bp = 500;
	//вспомогательная переменная для задания очередности действий
	var ocheredd_bp;
	//переменная, которая говорит о том, навели или увели мышь с панели
	//is_hover_t24_panel = "hover" - мышь наведена на панель;
	//is_hover_t24_panel = "out" - мышь уведена с панели;
	var is_hover_t24_panel;
	//переменная, которая говорит о том, на какую панель навели мышь
	// t24_panel_variant = t24_js_cabinet - мышь наведена на панель "личный кабинет"
	// t24_panel_variant = t24_js_vhod  - мышь наведена на панель "войти"
	var t24_panel_variant;

	/* ФУНКЦИИ */
	//тайминг, который начинает работать при наведении мыши на панель или при уведении мыши с панели
	function for_timming_busket_panel () {
		if (timer_stoop_bp == 0)  {
			ocheredd_bp = 0;
			if (ocheredd_bp == 0) {now_time_bp = new Date(); ocheredd_bp ++;}
			if (ocheredd_bp == 1) {current_time_bp = now_time_bp.getTime(); ocheredd_bp ++;}
			if (ocheredd_bp == 2) {zhdemms_bp = current_time_bp - start_time_ph; ocheredd_bp ++;}
			if (ocheredd_bp == 3) {ocheredd_bp ++;}
			if (ocheredd_bp == 4) {if (zhdemms_bp > predel_zhdemms_bp) {timer_stoop_bp = 1; zhdemms_bp = 0; t24_variant_panel ();}; ocheredd_bp ++;}
			if (ocheredd_bp == 5) {setTimeout(for_timming_busket_panel, 100);}
		}
	}

	//функция определяет вид панелей
	function t24_variant_panel () {
		//панель "личный кабинет"
		if (t24_panel_variant == "t24_js_cabinet") {
			//мышь наведена на панель
			if (is_hover_t24_panel == "hover") {
				$('.t24_js_bottom').removeClass('t24_js_activ_right').addClass('t24_js_activ_left');
				/* скроем селекты для ие6 */
				hide_select_in_ie6 ();
			};
			//мышь уведена с панели
			if (is_hover_t24_panel == "out") {
				$('.t24_js_bottom').removeClass('t24_js_activ_left');
				/* откроем селекты для ие6 */
				show_select_in_ie6 ();
			};
		};
		//панель "войти"
		if (t24_panel_variant == "t24_js_vhod") {
			//мышь наведена на панель
			if (is_hover_t24_panel == "hover") {
				$('.t24_js_bottom').removeClass('t24_js_activ_left').addClass('t24_js_activ_right');
				/* скроем селекты для ие6 */
				hide_select_in_ie6 ();
			};
			//мышь уведена с панели
			if (is_hover_t24_panel == "out") {
				$('.t24_js_bottom').removeClass('t24_js_activ_right');
				/* откроем селекты для ие6 */
				show_select_in_ie6 ();
			};
		};
