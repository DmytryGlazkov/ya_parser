        /* функция - меняет путь к картинке-заголовку блока */
        /* первый аргумент - объект-картинка (object_img),
        второй аргумент - способ замены (on_what) (on_red - текст должен стать белым ; on_black - текст должен стать черным) */
        function replase_katalog_img_path (object_img, on_what) {
            /* вынимаем путь к картинке */
            var katalog_img_path = object_img.attr('src');
            /* меняем его на новый */
            if (on_what == 'on_red') {
                var katalog_img_path_new = katalog_img_path.replace(/black/,'red');
            };
            if (on_what == 'on_black') {
                var katalog_img_path_new = katalog_img_path.replace(/red/,'black');
            };
            /* присваиваем новый путь */
            object_img.attr('src',katalog_img_path_new);
        }
	
