        /*ГОРИЗОНТАЛЬНОЕ МЕНЮ КАТАЛОГА С ВЫПАДАЮЩИМИ ПАНЕЛЯМИ */
	
        /* присваиваем/убираем класс t43_js_hover блоку .t43_js_link_container при наведении на него*/
        /* класс .t43_js_link_container дает раскрытую выпадающую панель и другой вид ссылки в меню при наведении */
		
        /* КОНСТАНТЫ И ПЕРЕМЕННЫЕ */
		
        //количество времени, которое прошло с начала отсчета таймера
        var zhdemms = 0; var zhdemms1 = 0;
        //метка времени в момент начала отсчета таймера
        var current_time; var current_time1;
        //метка текущего времени
        var current_time2;	var current_time21;
        //вспомогательные переменные для получения меток времени
        var now; var now1;
        //переменные, которые говорят о том, должен ли идти таймер если 1, то нет, если 0, то да
        var stoop = 1; var stoop1 = 1;
        //время ожидания, после которого показывается/убирается выпадающая панель
        var predel_zhdemms = 500; var predel_zhdemms1 = 500;
        //вспомогательная переменная для задания очередности действий
        var ocheredd; var ocheredd1;
		
        /* ФУНКЦИИ */
		
        //функция убирает выпадающие панели
        function remove_t43_js_hover () {
			$('.t43_js_katalog_menu .t43_js_link_container').removeClass('t43_js_hover');
			$('.t43_js_katalog_menu').removeClass('t43_js_hover_katalog_menu');
			/*  откроем селекты для ие6 */
            show_select_in_ie6 ();
			
			/* запустим баннеры */
			var slider_id = ( $('#slider').length > 0 ) ? 'slider' : 'slider_min';
			var slider = $('#' + slider_id).data('AnythingSlider');
			if (slider) {slider.startStop(true);};
        }
			
        //функция дает текущей панели класс t43_js_hover, и этот класс дает ей вид выпавшей панели
        function add_t43_js_hover () {
            $('.t43_js_katalog_menu .t43_js_link_container').removeClass('t43_js_hover');
			$('.t43_js_katalog_menu').addClass('t43_js_hover_katalog_menu');
            current_t43_js_link_container.addClass('t43_js_hover');
				
            var smenuID = current_t43_js_link_container.attr("id");
            smenuIDsub = smenuID.substr(5);
					
            if (smenuID != ""){
			
				/* count_td_65 - количество ячеек таблицы в меню */
				var count_td_65 = 4;
				/* count_blocks_65 - общее количество блоков в меню */
				var count_blocks_65 = smenu.aLevel2_counts[smenuIDsub];
				/* count_in_td_max_65 - наибольшее количество блоков в ячейке */
				var count_in_td_max_65 = Math.ceil(count_blocks_65/count_td_65);
				/* count_in_td_min_65 - наименьшее количество блоков в ячейке */
				var count_in_td_min_65 = Math.floor(count_blocks_65/count_td_65);
				/* ostatok_65 - остаток от деления количества блоков на количество ячеек */
				var ostatok_65 = count_blocks_65 % count_td_65;
				/* number_block_65 - после блока с каким номером нужно класть в ячейку по count_in_td_min блоков  */
                var number_block_65 = ostatok_65 * count_in_td_max_65;
				
				/* заполним smenuOptions полученными значениями для использования в меню */
				var smenuOptions = {
                    "count_td_65": count_td_65,
					"count_blocks_65": count_blocks_65,
					"count_in_td_max_65": count_in_td_max_65,
					"count_in_td_min_65": count_in_td_min_65,
					"ostatok_65": ostatok_65,
					"number_block_65": number_block_65,
                    "ID": smenuIDsub
                };
				
            }
            $('#mainMenuTemplate').tmpl(smenu, smenuOptions).appendTo(jQuery('#div'+smenuID).empty());
			
			/*  скроем селекты для ие6 */
            hide_select_in_ie6 ();
			/* остановим баннеры */
			var slider_id = ( $('#slider').length > 0 ) ? 'slider' : 'slider_min';
			var slider = $('#' + slider_id).data('AnythingSlider');
			if (slider) {slider.startStop(false);};
        }
			
        //тайминг, который начинает работать при наведении мыши на пункт меню
        function for_timming () {
            if (stoop==0)  {
                ocheredd = 0;
                if (ocheredd == 0) {now = new Date(); ocheredd ++;}
                if (ocheredd == 1) {current_time2 = now.getTime(); ocheredd ++;}
                if (ocheredd == 2) {zhdemms = current_time2 - current_time; ocheredd ++;}
                if (ocheredd == 3) {ocheredd ++;}
                if (ocheredd == 4) {if (zhdemms > predel_zhdemms) {stoop = 1; zhdemms=0; add_t43_js_hover ();}; ocheredd ++;}
                if (ocheredd == 5) {setTimeout(for_timming, 100); ocheredd ++;}
            }
        }
			
        //тайминг, который начинает работать при уведении мыши с пункта меню или выпавшей панели
        function for_timming1 () {
            if (stoop1==0)  {
                ocheredd1 = 0;
                if (ocheredd1 == 0) {now1 = new Date(); ocheredd1 ++;}
                if (ocheredd1 == 1) {current_time21 = now1.getTime(); ocheredd1 ++;}
                if (ocheredd1 == 2) {zhdemms1 = current_time21 - current_time1; ocheredd1 ++;}
                if (ocheredd1 == 3) {ocheredd1 ++;}
                if (ocheredd1 == 4) {if (zhdemms1 > predel_zhdemms1) {stoop1 = 1; zhdemms1=0; remove_t43_js_hover ();}; ocheredd1 ++;}
                if (ocheredd1 == 5) {setTimeout(for_timming1, 100); ocheredd1 ++;}
            }
        }
		
        /* РЕАКЦИЯ НА ДЕЙСТВИЯ ПОЛЬЗОВАТЕЛЯ */
		
        //
        $('.t43_js_katalog_menu .t43_js_link_container').hover(
        function() {
            current_t43_js_link_container = $(this);
            now = new Date();
