        /* поделим пункты списка на 2 колонки
        var part_1_t63_js_li = jQuery('.t63_js_catalog_ul_1 .t63_js_li').slice(0,bolshaya_polovina_t63_js_li).remove();
        var part_2_t63_js_li = jQuery('.t63_js_catalog_ul_1 .t63_js_li').slice(bolshaya_polovina_t63_js_li+1,vsego_t63_js_li-1).remove();
