<script type="text/javascript">
	$(document).ready(function() {
		var collapsed = 1,
			nav = $(".container-catalog-top"),
			buttonCatalogToggle = $(".main-catalog-toggle"),
			iconArrowClass = 'icon-arrow',
			nowEvent,
			menuStatus = false, 
			yetAnotherTimer;

		nav
			.menu({
				create: function(e, ui) {
					$(e.target)
						.find('.main-catalog-toggle span')
						.addClass(iconArrowClass+' icon-arrow-catalog')
						.end()
						.find('.catalog-item-lev1 > a > span')
						.addClass(iconArrowClass+' icon-arrow-right icon-arrow-gray')
						.before('<span class="ui-menu-icon icon-catalog"></span>')
						.end()
						.find(".group-toggle")
						.children('span')
						.first()
						.remove()
						.end()
						.off()
						.on("click", function(){
							$(this).toggleClass('active').parent().toggleClass('active')
							return false;
						});
				},
				icons: { submenu: "icon-arrow main-catalog-toggle-icon" }
			})
			.on('menufocus', function(e, ui) {
				if($(ui.item).children('.main-catalog-toggle').length) {
					nav.menu('option', "position", { my: "left top", at: "left bottom-5"});
				} else {
					nav.menu('option', "position", { my: "left top", at: "right top+1"});
				}
			});

		buttonCatalogToggle.on("click", function() {
			nav.toggleClass("active");
			return false;
		});

		$(window).on("mousemove", function(e){
			
			nowEvent = e;

			if( $(e.target).closest(".catalog-item-lev1").size() || $(e.target).closest(".catalog-item-lev2-container").size() ) {
				menuStatus = true;
			}
			
			if( menuStatus ) {

				if(yetAnotherTimer) clearTimeout(yetAnotherTimer);

				yetAnotherTimer = setTimeout(function(){

					if( !$(nowEvent.target).closest(".catalog-item-lev1").size() && !$(nowEvent.target).closest(".catalog-item-lev2-container").size() ) {
