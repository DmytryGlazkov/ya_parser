<script type="text/javascript">
    $(document).ready(function()
    {
        $("#reset_btn").click(function()
        {
            console.log ("click");
            $(this).addClass("disabled");
            $(this).attr("disabled","disabled");
            $(this).parents("form.aL1").trigger("submit");
        });

        $(".dop").live("click", function(){
            var text = $(this).text();
            $(".modal-header h3").text(text);
            $(".dop").css("opacity", "1").css("cursor", "pointer");
            $(this).css("opacity", "0.2").css("cursor", "default");
        });

       $(".alloy-login-state").click(function()
       {
            var toggle = $(this).attr('data-toggle');
            $("div[id^='alloy-']").hide();
            $("#"+toggle).show();
       });

       $("form.aL1").submit(function()
       {
           var url = $(this).attr('action');
           var delegate = $(this).attr('delegate');
           var data = $(this).serialize();
           var self = this;

           $.ajax({
               url:url,
               data: data,
               type:'POST',
               dataType:'json',
               success: function(opt)
               {
                   if (check_errors(opt) == false) eval(delegate)(opt,self);
               }
            });
           return false;
       });


        function set_session_id(opt, delegate)
        {
            var session_id = opt.session;
            $(".submit_form").addClass("disabled");
            $(".submit_form").text("Вход...");
            $("body").append("<script src='http://alloy.ru/?sid="+session_id+"' />");
            setTimeout(function(){delegate(opt)}, 2000);
        }

        function check_errors(opt)
        {
            $(".disabled").removeClass("disabled").removeAttr("disabled");
            if (opt.errors != undefined )
            {

                $('#errors').empty();
                $.each(opt.errors,function(e)
                {
                    $("#errors").show();
                    $("#errors").append("- "+ opt.errors[e]+"<br>");
                });
                return true;
            }

            return false;
        }

        function login(opt, o)
        {
            set_session_id(opt, redirect);
        }

        function redirect(opt)
        {
            if (opt.ans=="OK") { window.location.reload(); return; }
            if (opt.ans=="owner") { window.location = 'http://'+opt.host+'?__mode=editor'; }
        }

        function new_user(opt, o)
        {
            set_session_id(opt, redirect);
        }

        function reset_password(opt, o)
        {
            if (opt.ans=="OK")
            {
                $("#alloy-reset-form").hide();
                $("#alloy-reset-form").after('<div>На указанный email был выслан Ваш новый пароль');
            }
        }

    });
</script>

