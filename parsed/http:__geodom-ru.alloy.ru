    $(document).ready(function()
    {
        $("#ask-question").submit(function()
        {
            var self = this;
            $("#message-loader").show();
            $("#message-submit").hide();
            $.ajax({
                url:$(this).attr('action'),
                data: $(this).serialize(),
                type:'POST',
                dataType:'json',
                success: function(opt)
                {

                    if (opt[0] == "CAPCHAWRONG") {
                        $("input[name=messageid]").val(opt[1]);
                        $('#two #capcha').show();
                    }

                    if (opt[0] == "NOPHONE") {
                        $("input[name=messageid]").val(opt[1]);
                        $('#two.antispam').slideDown();
                    }

                    m_check_errors (opt);
                    if (opt == "OK")
                    {
                        $("#message-request-done").show();
                        $(self).hide();

                    }
                    else
                    {
                        $("#p-user-pass").val("");
                    }

                    $("#message-loader").hide();
                    $("#message-submit").show();
                }
            });


            return false;
        });

        function m_check_errors(opt)
        {
            $('#message-errors').empty();
            $("#p-user-pass").hide();
            $("#user-fio").show();

            if ($.isPlainObject(opt)){
                $.each(opt,function(e)
                {
                    var l = opt[e][0];
                    if (parseInt(l.substring(0,2))=='23')
                    {
                        $("#user-fio").hide();
                        $("#p-user-pass").show();
                        l = l.substring(3);
                    }
                    $("#message-errors").show();
                    $("#message-errors").append("- "+l+"<br>");
                });
                return true;
            }
            else
            {
                return false;
            }
        }
    });
</script>
