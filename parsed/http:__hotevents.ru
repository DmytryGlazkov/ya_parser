					var newtop = (($(window).height() - $("#overlay").height()) / 2+$(window).scrollTop()) + 'px';
					var newleft = (($(window).width() - $("#overlay").width()) / 2+$(window).scrollLeft()) + 'px';
					$('#overlay').css('position','absolute').css('top', newtop).css('left', newleft);	
	
	
					var maptop = (($(window).height() - $("#maplayer").height()) / 2+$(window).scrollTop()) + 'px';
					var mapleft = (($(window).width() - $("#maplayer").width()) / 2+$(window).scrollLeft()) + 'px';	
					$('#maplayer').css('position','absolute').css('top', maptop).css('left', mapleft);

					var xtop = ((($(window).height() - $("#maplayer").height()) / 2+$(window).scrollTop())) - $("#closepup").height() + 'px';
					var xleft = ((($(window).width() - $("#maplayer").width()) / 2+$(window).scrollLeft())) + $("#maplayer").width() - $("#closepup").width() + 'px';	
					$('#closepup').css('position','absolute').css('top', xtop).css('left', xleft);

					$('body').css('overflow','hidden');
                           
});
	

			 
$('#overlay').live('click',function(){
	$('body').css('overflow','auto');
	$("#maplayer").hide();
	$("#closepup").hide();
	$("#overlay").hide();
});
			 
$('#closepup').live('click',function(){
	$('body').css('overflow','auto');
	$("#maplayer").hide();
	$("#closepup").hide();
	$("#overlay").hide();
});
	
