	<script>function GL_loadSection(elem, container_id)
{	
	container_id = $.trim(container_id) || '';		
	
	if(container_id.length == 0)
		return;
	
	var container = $("#" + container_id);			
	
	if(container.hasClass('loading'))
		return;
	
	var _this = $(elem);
	
	if(_this.hasClass('active'))
		return false;

	var section_id = $(elem).data("id");		
	var section_tab = container.find('.tab-body#tab' + section_id + ':first');					

	container.find('.tab-head.active:first').removeClass('active');
	_this.addClass('active');
	
	container.find('.tab-body.showed:first').addClass('hidden').removeClass('showed');
	section_tab.removeClass('hidden').addClass('showed');				
	
	if(!_this.hasClass('loaded'))
	{		
		var params = {
			AJAX_CALL: 'Y',
			ACTION: 'goods_list',
			SECTION_ID: section_id,
			MODE: container.data("mode")
		};			
		
		if(typeof(BX) != 'undefined' && typeof(BX.bitrix_sessid) != 'undefined')
			params['sessid'] = BX.bitrix_sessid();			
		
		showProgressBar("#" + container_id + ' .tab-body#tab' + section_id, container.data("mode"), 100);
		
		container.addClass('loading');
		
		$.post(
			container.data("url"),
			params,
			function(html){			
				section_tab.replaceWith(html);
				_this.addClass("loaded");
				hideProgressBar(container.data("mode"));
				
				$("#" + container_id + ' .tab-body#tab' + section_id).find("select.page_size").sb();
				
				initCheckboxStyle("#" + container_id);
				
				container.removeClass('loading');
			},
			'html'
		);			
	}
	
	return false;
}</script>

