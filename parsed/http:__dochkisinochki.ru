<script>
    var top_banner = {
            i: 1,
            timer: 10000,
            stop: 5,
            go: true,
            start: function(){
                if(this.go && this.stop>0){
                    try{
                        setTimeout('top_banner.handler()', this.timer);
                    } catch(e) {
                        this.i = 1;
                        setTimeout('top_banner.start()', this.timer);
                    }
                }
            },
            handler: function(){
                if(this.go && this.stop>0){
                    $('.slidebutton-wrapper').removeClass('active_sld');
                    $('#banner_slidebutton_wrapper'+this.i).addClass('active_sld');
                    tab('banner', this.i);
                    if( this.i == this.stop ){
                        this.i = 1;
                    }else {
                        this.i++;
                    }
                    this.start();
                }
            }
        }
    $(function(){
        top_banner.start();
        $('.slidebutton').click(function(){
            top_banner.go = false;
        });
    });
</script>        
