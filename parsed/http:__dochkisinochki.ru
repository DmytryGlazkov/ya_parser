        <script>

            //  Устанавливаемый location.hash
                var page_hash = "";

                //console.log(window.location);

            //  Условие для проверки, если location.hash не пустой
                if (window.location.hash != "")
                {
                    //  Сохраним первоначальное значение location.hash
                        page_hash = window.location.hash;

                    //  Произведем замену location.hash
                        window.location.hash = "";
                }

            //  Функция подмены Hash в Location
                function changeHash(e,obj)
                {
                    var dest;
                    if (location.pathname.search(obj.pathname) != -1)
                    {
                        dest = obj.hash;
                        if ( dest != "#" && dest.search("ym_") == -1 && dest.search("&") == -1 )
                        {
                            if ($(dest).length)
                            {

                                e.preventDefault();
                                window.scroll(0,($(dest).offset().top-45));
                            }
                            else
                            {


                                window.scroll(0,($("a[name=" + dest.replace("#","")+"]").offset().top-45));
                            }
                        }
                    }
                }

            //  Обработчик на загрузку страницы
                window.onload = function()
                {
                    var a = document.links;

                    for (i = 0; i < a.length; i++)
                    {
                        if ( (a[i].href.indexOf('#') + 1) && (a[i].hash != "") )
                        {
                            $(a[i]).click( function(e){
                                changeHash(e,this)
                            });
                        }
                    }

                    if ( page_hash != "" )
                    {
                        if ( $(page_hash).length )
                        {
                            window.scroll(0,($(page_hash).offset().top-45));
                        }
                        else
                        {
                            window.scroll(0, ($("a[name=" + page_hash.replace("#","") + "]").offset().top - 45));
                        }
                    }
                }

        </script>

