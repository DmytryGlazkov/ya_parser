						var delta = 0;
						var newWidth = 0;
						if(width > minOutterWidth){
							if(availableItem > numberItem){
								delta = Math.floor((width - minOutterWidth * numberItem) / numberItem);
							}else {
								delta = Math.floor(width % minOutterWidth / availableItem);
							}
							newWidth = minWidth + delta;
