<script type="text/javascript">
function nq_add()
{
	$('#nq_table').append('<tr class="nq_update">' + $('#nq_table tr:last').html() + '</tr>');
	$('.nq_remove').css('visibility', 'visible');
	$('#nq_table tr:last select').val('');
	$('#nq_table span').css('visibility', 'visible');
	$('#nq_table tr:last span').css('visibility', 'hidden');
	$('#nq_table tr:last input').attr('value', 1);
	if ($('#nq_table').children().children().length == 2)
	{
		$('.nq_remove').css('visibility', 'hidden');
	}
	$('#nq_table tr:last .nq_remove').css('visibility', 'hidden');
}

function nq_remove(nq)
{
	$(nq).parent().parent().remove();
	if ($('#nq_table').children().children().length == 2)
	{
		$('.nq_remove').css('visibility', 'hidden');
	}
	nq_update();
}

function nq_nominal_change(nq)
{
	if (typeof nq.not_first == 'undefined')
	{
		nq_add();
		nq.not_first = true;
	}
	nq_update();
}

function nq_update()
{	
	var sum = 0;
	$('.nq_update').each
	(
		function()
		{
			var select = $('select', this);
			var input = $('input', this);
			if (!isNaN(parseInt(select.val()))) sum += parseInt(select.val()) * parseInt(input.attr('value'));
		}
	);
	if (isNaN(sum)) $('#nq_sum').html('');
	else $('#nq_sum').html(sum + ' руб.');
}

function order_certificate()
{
	var success = true;
	$('#dialog input[type=text]').each
	(
		function()
		{
			if ($(this).attr('name') != 'comment' && $(this).attr('value') == '') success = false;
		}
	);
	if (!success) alert('Заполните все необходимые поля формы!');
	else
	{
		var nq = '';
		$('.nq_update').each
		(
			function()
			{
				var nominal = parseInt($('select', this).val());
				var quantity = parseInt($('input', this).attr('value'));
				if (!isNaN(nominal) && !isNaN(quantity))
				{
					nq += nominal + ' руб. - ' + quantity + " шт.\r\n";
				}
			}
		);
		$('#dialog input[name=nq]').attr('value', nq);
		$('#dialog input[name=phone]').attr('value', $('#dialog input[name=phone3]').attr('value') + ' ' + $('#dialog input[name=phone1]').attr('value')+ $('#dialog input[name=phone2]').attr('value'));
		$('#dialog input[name=mail]').attr('value', $('#dialog input[name=mail1]').attr('value') + '@' + $('#dialog input[name=mail2]').attr('value'));

		var captcha_word = $('#dialog input[name=captcha_word]').attr('value');
		var captcha_code = $('#dialog input[name=captcha_code]').attr('value');
		var url = '/extra/cards/?type=retail&cert_check_captcha=true&captcha_word=' + captcha_word + '&captcha_code=' + captcha_code;
		$.get
		(
			url,
			function(data)
			{
				if (data == 'false') alert('Вы ввели неправильный код!');
				else if (data == 'true')
				{
					alert('Ваш заказ отправлен! С Вами свяжется наш специалист. Спасибо!');
					$('#dialog form').submit();
				}
			}
		);
	}
}
</script>


