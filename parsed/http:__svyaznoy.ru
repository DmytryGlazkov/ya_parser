<script type="text/javascript">

    $(document).ready(function(){
        (function(){

            function validateName(name) {
                return $.trim(name).length > 1;

            }

            function validateEmail(email) {
                var reg = /^([A-Za-z�-��-�0-9_\-\.])+\@([A-Za-z�-��-�0-9_\-\.])+\.([A-Za-z�-��-�]{2,4})$/;
                if(!reg.test(email)) {
                    return false;
                }

                var addPater = [];
                addPater[0] = /(.*)mail$/; addPater[1] = /(.*)test\.ru$/; addPater[2] = /(.*)site\.ru$/;                 for(var i=0; i<addPater.length; i++) {
                    if( addPater[i].test(email) ) {
                        return false;
                    }
                }
                return true;
            }

            function validatePassword(password) {
                return password.length >= 1;
            }

            function validatePasswords(password1, password2) {
                return password1 == password2;
            }

            // ������ ������ �����
            var $form = $('#popupRegisterForm');
            $form.find('.regButton').click(function(){
                $form.submit();
            });
            $form.keypress(function(e){
                if(e && e.keyCode == 13) {
                    $form.submit();
                }
            });

            // ���������� ������ �����
            $form.submit(function(){
                var email =     $form.find('tr[data-field=email] input').val().trim();
                var password =  $form.find('tr[data-field=password] input').val().trim();
                var password2 = $form.find('tr[data-field=password2] input').val().trim();
                var name =      $form.find('tr[data-field=name] input').val().trim();
                var lastname =  $form.find('tr[data-field=lastname] input').val().trim();
                var subscribe = $form.find('tr[data-field=subscribe] input').attr('checked') ? 'on' : '';

                if(!validateEmail(email)){
                    popupShowFieldError($form, 'email', "����������, ������� ���������� ����� ����������� �����");
                    return false;
                }

                if(!validatePassword(password)){
                    popupShowFieldError($form, 'password', "������ ����������");
                    return false;
                }

                if(!validatePassword(password2)){
                    popupShowFieldError($form, 'password2', "��������� ������");
                    return false;
                }

                if(!validatePasswords(password, password2)){
                    popupShowFieldError($form, 'password2', "��������� ������ �� ���������");
                    return false;
                }

                if(!validateName(name)){
                    popupShowFieldError($form, 'name', "����������, ������� ���");
                    return false;
                }

                var request = {
                    login: email,
                    email: email,
                    password1: password,
                    password2: password2,
                    name: name,
                    lastname: lastname,
                    us_subscribe: subscribe
                };

                $.ajax({
                    url: '/ajax/new/reg_auth.php',
                    type: 'POST',
                    dataType: 'html',
