<script type="text/javascript">
var short_basket_timer = 0;
var short_basket_loading = false;
var showShortBasket = function() {
    if (!short_basket_loading)
    {
        var hash = $.cookie('BASKET_HASH');
        var old_hash = $('.b-menu__basket__product').attr('rel');

        if (old_hash == '' || old_hash != hash)
        {
            short_basket_loading = true;
            var timer = setTimeout('short_basket_loading = false', 1000);
            $('.b-menu__basket__product').html('<div style="margin-left: 43%"><img width="36" height="36" title="preload" src="/img/loader_b.gif"></div>');
            $('.b-menu__basket-show_hide').toggleClass('b-menu__basket-full b-menu__basket-act');
            $(".b-menu__basket__product").show();

            $.post(
                '/ajax/cart/get_short_cart.php',
                {short_cart: 'Y'},
                function(res) {
                    if (res) {
                        $('.b-menu__basket__product').html(res);
                        $('.b-menu__basket__product').attr('rel', hash);
                        if (short_basket_timer) {
                            clearTimeout(short_basket_timer);
                            short_basket_timer = 0;
                        };
                    }
                },
                'html'
            );
        }
        else
        {
            $('.b-menu__basket-show_hide').toggleClass('b-menu__basket-full b-menu__basket-act');
            $(".b-menu__basket__product").show();
            if (short_basket_timer) {
                clearTimeout(short_basket_timer);
                short_basket_timer = 0;
            };
        }
    }
};
$(document).ready(function(){
    /* open/close basket */
    $('.b-menu__basket-show_hide').live('mouseenter', showShortBasket).live('mouseleave', function() {
        short_basket_timer = setTimeout(function(){
            $(".b-menu__basket__product").hide();
            $('.b-menu__basket-show_hide').toggleClass('b-menu__basket-full b-menu__basket-act');
        }, 100);
    });
    $('.b-menu__basket-show_hide .product-btm__btn a').live('click',function(e){
        _gaq.push(['_trackEvent', 'proceedtocart', 'click', '']);
    });

});
    
/* open/close menu */
(function(){
    var is_index = 1;
    var prev_change_scroll = false;
    var menu_top = $('.b-menu').offset().top;
    var menu_left = $('.b-menu').offset().left;
    var timeout = ($.browser.msie  && parseInt($.browser.version, 10) <= 8) ? 5 : 0;

    function toggle_catalog(has_show) {
        var show = $('.b-menu__menu-int').css('display') != 'none';
        if (has_show != show) {
            $('.menu-internet').toggleClass('b-menu__menu-internet-act b-menu__menu-internet');
            $('.b-menu__menu-int').toggle();
        }
    }

    function toggle_services(has_show) {
        var show = $('.b-menu__bl-services').css('display') != 'none';
        if (has_show != show) {
            $('.menu-services').toggleClass('b-menu__menu-services-act b-menu__menu-services');
            $('.b-menu__bl-services').toggle();
        }
    }

    var internet_time;
    $('.menu-internet, .b-menu__menu-int').hover(function(){
        clearTimeout(internet_time);
        internet_time = setTimeout(function(){
            toggle_catalog(true);
            toggle_services(false);
        }, timeout);
    }, function(e){
        clearTimeout(internet_time);
        internet_time = setTimeout(function(){
            if (!is_index) {
                toggle_catalog(false);
            }
        }, timeout);
    });

    var service_time;
    $('.menu-services, .b-menu__bl-services').hover(function(){
        clearTimeout(service_time);
        service_time = setTimeout(function(){
            toggle_services(true);
            toggle_catalog(false);
        }, timeout);
    }, function(){
        clearTimeout(service_time);
        service_time = setTimeout(function(){
            toggle_services(false);
            if (is_index) {
                toggle_catalog(true);
            }
        }, timeout);

    });

    if ($(window).width() >= 990 && !navigator.userAgent.match(/(iPhone|iPod|iPad|BlackBerry|Android|Opera Mini|webOS)/)) {
        $('ul.b-menu__menu-int-list .menu1-list__bl').hover(function(){
            $(this).find('.b-menu__bl-internet').slideToggle(timeout);
            if($(this).is(':not(.menu1-list__bl-link)')) {
                $(this).find('.menu1-list_border').toggle();
            }
        });
    }

    if (is_index) {
        toggle_catalog(true);
    }
})();
</script>	</div>

