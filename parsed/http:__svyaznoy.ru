<script>
	jQuery(document).ready(function() {
		jQuery('#send_account_info').click(function() {
			jQuery('#send_account_info').hide();
			if(jQuery('input[name=pwd_USER_LOGIN]').val().length == 0) {
				jQuery('.err_section_first').text('������� email');
				jQuery('#send_account_info').show();
				return false;
			} else {
				jQuery('.err_section_first').text('')
			}
			
			jQuery.post('/forgot/action.php',
			{
				action: 'forgot_password',
				AUTH_FORM: 'Y',
				TYPE: 'SEND_PWD',
				USER_LOGIN: jQuery('input[name=pwd_USER_LOGIN]').val(),
				captcha_sid: jQuery('input[name=captcha_sid]').val(),
				captcha_word: jQuery('input[name=captcha_word]').val(),
				cur_page: window.location.pathname
			}, function(data) {
                if(data) {
                    if(data.TYPE == 'OK')
                    {
                        jQuery('#send_to').text(data.EMAIL);
                        jQuery('#fancybox-close').click();

                        if(data.SENT == true) {
                            jQuery('.pasRecovery').find('.pasSent').show();
                        }else{
                            jQuery('.pasRecovery').find('.pasSent').hide();
                        }

                        if(data.SOCIAL_NETWORKS.length > 0) {
                            var $socialButtons = jQuery('#auth_form').find('.social_auth_button');
                            $('.social-icon').addClass(data.SOCIAL_NETWORKS[0]);

                            jQuery('.pasRecovery').find('.social_auth_button.all').html($socialButtons.html());

                            jQuery('.pasRecovery').find('.social_auth_button.one').html('');
                            jQuery.each(data.SOCIAL_NETWORKS, function(k, v){
                                jQuery('.pasRecovery').find('.social_auth_button.one').append($socialButtons.find('.'+v));
                            });

                            jQuery('.pasRecovery').find('.pasSocialNetworks').show();
                        }else{
                            jQuery('.pasRecovery').find('.pasSocialNetworks').hide();
                        }

                        if(data.SOCIAL_NETWORKS.length > 0 && data.SENT == true) {
                            jQuery('.pasRecovery').find('.also').show();
                        }

                        if(data.SOCIAL_NETWORKS.length > 0 && data.SENT == false) {
                            jQuery('.pasRecovery').find('.reg').show();
                        }

                        jQuery('#forgotPas2').click();
                        jQuery('#send_account_info').show();
                        reloadCaptcha();
                    }
                    else
                    {
                        jQuery('.err_section_first').html(data.MESSAGE);
                        jQuery('#send_account_info').show();
                        reloadCaptcha();
                    }
                }
			}, 'json');
		});
		
		jQuery('#change_pwd').click(function() {
			jQuery('#change_pwd').hide();
			jQuery.post('/forgot/action.php',
			{
				action: 'chg_password',
				AUTH_FORM: 'Y',
				TYPE: 'CHANGE_PWD',
				USER_LOGIN: jQuery('input[name=chg_pwd_USER_LOGIN]').val(),
				USER_CHECKWORD: jQuery('input[name=chg_pwd_USER_CHECKWORD]').val(),
				USER_PASSWORD: jQuery('input[name=pwd_USER_PASSWORD]').val(),
				USER_CONFIRM_PASSWORD: jQuery('input[name=pwd_USER_CONFIRM_PASSWORD]').val()
			}, function(data) {				
				var obj = data.split('**|**');	
				if(obj[1] == 'OK')
				{
					location.reload(true);
				} 
				else
				{
					jQuery('.err_section').html(obj[0]);
					jQuery('#change_pwd').show();
				}
			});
		});		
		
		if(jQuery.cookie('chgpwd_USER_CHECKWORD') !== null 
		&& jQuery.cookie('chgpwd_USER_LOGIN') !== null
		&& jQuery.cookie('chgpwd_USER_LOGIN') !== 'null'
		&& jQuery.cookie('chgpwd_USER_LOGIN') !== 'null'
		&& jQuery.cookie('chgpwd_USER_LOGIN') !== undefined)
		{
			var trueLogin = jQuery.cookie('chgpwd_USER_LOGIN');
            if (trueLogin) {
                trueLogin = trueLogin.replace(/\+/, ' ');
            }


			jQuery('#forgotPas3').click();
			jQuery('input[name=chg_pwd_USER_LOGIN]').val(trueLogin);
			jQuery('input[name=chg_pwd_USER_CHECKWORD]').val(jQuery.cookie('chgpwd_USER_CHECKWORD'));
		
			jQuery.cookie("chgpwd_USER_LOGIN","null", {
				expires: 7,
				path: "/"	
			});
			
			jQuery.cookie("chgpwd_USER_CHECKWORD","null", {
				expires: 7,
				path: "/"	
			});
		}

		
		jQuery('#forgotPas1').click(function() {
			reloadCaptcha();
		});
		
		jQuery('#goBackPwd').click(function() {
			jQuery('#forgotPas2').hide();
			jQuery('#forgotPas1').click();
			return false;
		});
	});
	
	function reloadCaptcha()
	{
		jQuery('.inputCaptcha').val('');
		jQuery.post('/forgot/action.php',
		{
			action: 'getCapCode'
		}, function(data) {
			jQuery('#pwdCaptchaImg').html('<input type="hidden" name="captcha_sid" value="'+data+'"><img src="/bitrix/tools/captcha.php?captcha_sid='+data+'">');
		});	
	}
</script>

