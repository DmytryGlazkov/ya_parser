<script type="text/javascript">
    setMiniCart = function () {
        $(document).ready(function () {
            var counter = true;

            $('#m-cart').on({
                mouseenter: function () {
                    if(parseInt($("#mcart").attr('data-count')) > 0) {
                        if (counter) {
                            counter = false;
                            $('#m-cart .popup-cart').slideDown(10, function () {
                                counter = true;
                            })
                        }
                    }
                },
                mouseleave: function () {
                    if(parseInt($("#mcart").attr('data-count')) > 0) {
                        if (counter) {
                            counter = false;
                            $('#m-cart .popup-cart').slideUp(10, function () {
                                counter = true;
                            })
                        }
                    }
                }
            });
        });
    };


    _initializeMiniCart = function () {
        App.miniCartRegion.show(App.layoutMiniCart);
        App.layoutMiniCart.header.show(new MiniCartHeaderView({model: Repository.cartModel, collection: Repository.cartModel.getItems('items')}));
        App.layoutMiniCart.content.show(new MiniCartContentView({model: Repository.cartModel, collection: Repository.cartModel.getItems('items')}));
        setMiniCart();
    };


