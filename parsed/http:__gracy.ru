			$('#ORDER_PROP_23').val($(this).attr('data-d')); 
		})
		
		$('form[name="SIMPLE_FORM_1"]').live('submit', function(){
			var url = $(this).attr('action');
			var data = $(this).serialize();
			var b = $(this);
			$.ajax({
				url: url,
				type: 'post',
				data: data,
				cache: false,
				success: function (data) {
					$(b).html(data);
			 
				}
			});
			return false
		})
