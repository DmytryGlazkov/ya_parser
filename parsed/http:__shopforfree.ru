            <div class="tab-pane" id="review"><script type="text/javascript">
    $(document).ready(function()
    {
       $("#bad_or_good_link").click(function(){
            $("#bad_or_good").toggle();
       });


        $("#antispam-phone-done").click(function()
        {
            var phone_number = $("#antispam-phone");

            if (phone_number.val()=='' || phone_number.val()==phone_number.attr('placeholder'))
            {
                alert('Вы забыли указать телефон');
            }

        });

        function validate_email(email) {
            var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
            return reg.test(email);
        }


        $("#new-review-frm").submit(function()
        {
            var self = this;
            var data = {};
            


                var email_obj = $(this).find('input[name=email]');
                var fio_obj = $(this).find('input[name=fio]');

                if ((email_obj.val() == email_obj.attr('placeholder')) || (!validate_email(email_obj.val()))) {
                    email_obj.css('border','1px solid red');
                    return false;
                }

                data.email = email_obj.val();
                data.fio = fio_obj.val();
                if (data.fio == fio_obj.attr('placeholder')) data.fio = '';

                if ($(this).find('.email-exists').css('display')=='block')
                {
                    data.email = $(this).find('input[name=email-s]').val();
                    data.password = $(this).find('input[name=pass]').val();
                }
            

            data.title = $(this).find('input[name=title]').val();
            data.what_like = $(this).find('textarea[name=what_like]').val();
            data.what_dislike = $(this).find('textarea[name=what_dislike]').val();
            data.comment = $(this).find('textarea[name=comment]').val();
            data.doomsday = $(this).find('input[name=doomsday]').val();
            data.score = $(this).find('select[name=score]').val();
            data.reviewid = $(this).find('input[name=reviewid]').val();

            var phone = '';
            if ($(this).find('input[name=phone]').val() != '' && $(this).find('input[name=phone]').val() != $(this).find('input[name=phone]').attr('placeholder'))
            {
                phone = $(this).find('input[name=phone]').val();
            }

            data.phone = phone;
            data.capcha = $(this).find('input[name=capcha]').val();

            var url = $(this).attr('action');

            $(this).find('.progress').show();
            $(this).find('input[type=submit]').hide();
            $.ajax({
                url:url,
                type: 'POST',
                data: data,
                dataType:'json',
                success:function(data)
                {
                    $(self).find('.progress').hide();
                    $(self).find('input[type=submit]').show();

                    if (typeof data != "object")
                    {
                        if (data == 'SPAM')
                        {
                            alert ('Отзыв не добавлен по одной из причин:\n- Сообщение содержит ссылки\n- Это спам');
                        }

                        var auth_error_exists = data.substring(0,5);
                        if (auth_error_exists=='error')
                        {
                            var auth_error = data.substring(6);
                            var auth_error_detail = auth_error.split('-');
                            if ($.trim(auth_error_detail[0]) == "email_exists")
                            {
                                $(self).find("b.user-name").text($.trim(auth_error_detail[1]));
                                $(self).find(".email-exists").show();
                                $(self).find(".email-dn-exists").hide();
                                $(self).find("input[name=email-s]").val($(self).find("input[name=email]").val())
                                $(self).find("input[name=pass]").focus();
                            }

                            if ($.trim(auth_error_detail[0]) == "username_failed")
                            {
                                alert('Представьтесь пожалуйста.');
                                $(self).find("input[name=fio]").focus();
                            }

                            if ($.trim(auth_error_detail[0]) == "login_failed")
                            {
                                alert('Указанный email и пароль не совпадают.')
                            }

                            return
                        }
                    }
                    else
                    {
                        if (data[0] == "CAPCHAWRONG")
                        {
                            $("input[name=reviewid]").val(data[1]);
                            $('#tree #capcha').show();
                        }

                        if (data[0] == "NOPHONE")
                        {
                            $("input[name=reviewid]").val(data[1]);
                            $('#tree.antispam').slideToggle();
                        }

                    }

                    if (data=='success')
                    {
                        $("#new-review-frm").html('Спасибо за Ваш отзыв!');
                        return;
                    }

                    if (data.comment)
                    {
                        alert(data.comment);
                        $(self).find('textarea[name=comment]').focus();
                    }



                }

            })
            return false;
        });

    });
</script>
