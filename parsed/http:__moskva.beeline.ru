<script type="text/javascript">
     //список клбчевых слов для фильтрации
     var paramWords = ["Билайн","дома","с собой"];;
     var selectSectionKey = "section"; 

     $(document).ready(function () {
         $("#quickLinksTitle span.dynamic").click(function () {
             setActiveQuickLink($(this));
             QA.Beeline.GoogleAnalytics.quickLinksClick($(this).text());
         }
         );

         $(".anchor").click(function (event) {
             event.preventDefault();
             var full_url = this.href;
             var parts = full_url.split("#");
             var trgt = parts[1];
             var target_offset = $("#" + trgt).offset();
             if (target_offset) {
                 var target_top = target_offset.top;
                 $('html, body').animate({ scrollTop: target_top }, 500, '');
             }
         });

         var sectionTitle = $.url(location.href).param(selectSectionKey); //Если надо активировать вкладку в быстрых ссылках

         var selected = false;
         //Если в ссылке предвыбрана секция
         if(sectionTitle != undefined && sectionTitle != null && sectionTitle != ""){
             $("#quickLinksTitle span").each(function () {
                 if ($(this).text().toLowerCase() == sectionTitle.toLowerCase()) 
                 {
                     setActiveQuickLink($(this));
                     selected = true;
                     return false;
                 }
             });
         }

         //Если секция не предвыбрана или не найдена, ищем дефолтную
         if(selected === false){
             $("#quickLinksTitle span").each(function () {
                 if ($(this).text() == "Билайн") $(this).removeClass("dynamic").addClass("highlight");
            });
        }
    });

    function setActiveQuickLink($element){
        var keyword = $element.text();
        var key_index = getIndexCaseInsensitive(keyword);//$.inArray(keyword, paramWords);

        //скрываем группы в первом блоке с sidebar
        $('div[id^="FirstLinksBlock-"]').each(function (index) {
            if ($(this).attr('id').replace("FirstLinksBlock-", "") == key_index.toString()) {
                $(this).show();
            }
            else {
                $(this).hide();
            }
        });
        //скрываем группы в блоках ниже если они есть
        $('div[id^="OtherLinksBlock-"]').each(function (index) {
            if ($(this).attr('id').substr($(this).attr('id').length - 1) == key_index.toString()) {
                $(this).show();
            }
            else {
                $(this).hide();
            }
        });

        //меняем выделения в ключевой фразе
        $("#quickLinksTitle span.highlight").removeClass("highlight").addClass("dynamic");
        $element.removeClass("dynamic").addClass("highlight");
    }

    function getIndexCaseInsensitive(keyword){
        var matchString = keyword.toLowerCase();

        var rslt = -1;
        $.each(paramWords, function(index, value) { 
            if (rslt == -1 && value.toLowerCase() === matchString) {
                rslt = index;
                return false;
            }
        });
        return rslt;
    }
</script>

