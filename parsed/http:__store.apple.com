<script>
    var plf = new apple.PagelevelFeedbackViewController($('plf'), {
    "lowCharsMessage":"Characters remaining before maximum limit is reached  {0}",
    "errorMessage":"An error has occurred. Please try again later.",
    "serviceURL":"/us/getx/feedback?nodePath=standard/home",
    "ratingQuestionTemplate":"{{AsRatingQuestion}}",
    "nodePath":"standard/home",
    "noCharsMessage":"Characters limit has been exceeded by {0}, Please reduce the length of your message before continuing.",
    "loadingMessage":"Loading",
    "ratingsLabelTemplate":"{0} out of {1} stars",
    "maxCharsAllyLabel":"Characters in this field are limited to {0}",
    "confirmationMessage":"Thanks for sharing your thoughts with us.",
    "partNumber":"",
    "freeTextQuestionTemplate":"{{AsWebFreeTextQuestion}}"
    });
</script>
<script type="text/mustache" id="{{AsRatingQuestion}}">
<legend class="question" id="{{id}}-legend">{{{description}}}</legend>
<span class="star-wrapper">
{{#stars}}
<label id="{{id}}-{{value}}-label" for="{{id}}-{{value}}"><span class="a11y"><input id="{{id}}-{{value}}" aria-labelledby="{{id}}-legend" type="radio" value="{{value}}" name="{{id}}-group">{{label}}</span></label>
{{/stars}}
</span>
</script>
<script type="text/mustache" id="{{AsWebFreeTextQuestion}}">
<label id="{{id}}-label" class="a11ylabel {{labelClass}}" for="{{id}}-textarea">{{description}}
<span class="a11y">{{maxCharsAllyLabel}}</span>
</label>
<textarea id="{{id}}-textarea" cols="30" rows="6"></textarea>
<span id="{{id}}-count" class="count {{charsRemainingClass}}">{{charsRemaining}}</span>
<p id="{{id}}-count-msg" class="a11y">{{lowCharsMessage}}</p>
</script>




