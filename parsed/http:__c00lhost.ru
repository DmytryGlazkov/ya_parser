$("div.shop").each(function(i){
	var wind = $("div.shop-pane").width();
	var inrow = 4;
	if(wind >= 1473)  {	inrow = 6;}
	if(wind > 1145 && wind < 1473)  {	inrow = 5;}
	if(wind < 1305)  {	inrow = 4;}
	var space = Math.round((wind - $(this).width() * inrow) / (inrow -1));
	var scrollRight =  Math.round(wind - $(this).position().left - $(this).width() - space);
	if(scrollRight > space) $(this).css("margin-right",space+"px");

});
