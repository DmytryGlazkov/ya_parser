    <script>
	//красиво перемещает эл-т elt предварительно быстро сметив на dx, dy и красиво возвращает 
	//к изначальным координатам за время time ф-ей animate
	function animateMove(elt, dx, dy, time)
	{
		//запомниаем координаты
		x = parseInt(elt.css('left'));
		y = parseInt(elt.css('top'));
		//смещаем на координаты перемещения
		elt.css({'visibility':'hidden'});
		elt.css({'left': x - dx});
		elt.css({'top' : y - dy});
		//перемещаям анимированно
		elt.css({'visibility':'visible'});
		elt.animate({'left' : x,'top' : y}, time);
	}
	
