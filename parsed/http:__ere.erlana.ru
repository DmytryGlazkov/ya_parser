<!-- Маска, затемняющая фон -->
<div id="mask"></div>



<script>
function ShowIBWindow(){
		var maskHeight = $(document).height();
		var maskWidth = $(window).width();
		$("#mask").css({"width":maskWidth,"height":maskHeight});
		$("#mask").fadeIn(1000); 
		$("#mask").fadeTo("slow",0.8); 
		var winH = $(window).height();
		var winW = $(window).width();
		$("#dialog").css("top",  winH/2-$("#dialog").height()/2);
		$("#dialog").css("left", winW/2-$("#dialog").width()/2);
		$("#dialog").fadeIn(2000); 
	}
	
	function closeIPWindow(){
		$("#mask, .window").hide();
	}

$(document).ready(function() {   
    $(".window .close").click(function (e) { 
		e.preventDefault();
		closeIPWindow();
    }); 
    $("#mask").click(function () {
		 closeIPWindow();
    }); 
	// alert('a');
});  

   
$(".getsale").click(function(e){
	e.preventDefault();
	nam= $(".nam").val();
	tel= $(".tel").val();
	
	if(!nam) {
		alert("Представьтесь, пожалуйста");
		return false;
	}
	if(!tel) {
		alert("Как нам с Вами связаться?");
		return false;
	}
	console.log("/bitrix/js/ipol.basket/reg.php?tel="+tel+"&nam="+nam);
	$.ajax({
	  url: "/bitrix/js/ipol.basket/reg.php?tel="+tel+"&nam="+nam,
	  type: "GET",
	  //dataType:"json",
	  success: function(dat){
		
		alert(dat);
		if($(this).attr("href") && $(this).attr("href")!="javascript:void(0);")
		{
			window.location($(this).attr("href"));
		}
		else
			closeIPWindow();
	  },
	  error: function(data,b,c){

	  }
	});
	
});
</Script>

