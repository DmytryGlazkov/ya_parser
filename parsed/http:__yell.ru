var slideWidth = 244;
var startMargin = "-488px";
var isSliding = false;
var topFontpageSliderTimeout = null;
$(document).ready(function() {
	$("#frontpage_top_arrow_left").click(function() {
		clearTimeout(topFontpageSliderTimeout);
		topFontpageSliderTimeout = null;
		moveLeft();
		return false;
	});
	
	$("#frontpage_top_arrow_right").click(function() {
		clearTimeout(topFontpageSliderTimeout);
		topFontpageSliderTimeout = null;
		moveRight();
		return false;
	});

	topFontpageSliderTimeout = setTimeout(moveRight,10000);
});

function moveLeft() {
	if(isSliding) {
		return false;
	}
	isSliding = true;

	var left = parseInt($("#frontpage_top_slider").css("margin-left")) + slideWidth;
	
	$("#frontpage_top_slider").animate(
		{"margin-left":left+"px"},
		300, 'linear', function() {
			var $a = $("#frontpage_top_slider_inner > a:last-child").clone();
			$("#frontpage_top_slider_inner").prepend($a);
			$("#frontpage_top_slider_inner > a:last-child").remove();
			$("#frontpage_top_slider").css("margin-left",startMargin);
			isSliding = false;
			topFontpageSliderTimeout = setTimeout(moveRight,10000);
		}
	);
	
}

function moveRight() {
	if(isSliding) {
		return false;
	}
	isSliding = true;

	var left = parseInt($("#frontpage_top_slider").css("margin-left")) - slideWidth;
	
	$("#frontpage_top_slider").animate(
		{"margin-left":left+"px"},
		300, 'linear', function() {
			var $a = $("#frontpage_top_slider_inner > a:first-child").clone();
			$("#frontpage_top_slider_inner").append($a);
			$("#frontpage_top_slider_inner > a:first-child").remove();
			$("#frontpage_top_slider").css("margin-left",startMargin);
			isSliding = false;
			topFontpageSliderTimeout = setTimeout(moveRight,10000);
		}
	);
	
}
</script>

