<script>
	(function(exec){
		if ('function' === typeof require) {
			require([ 'jquery', 'underscore' ], exec);
		} else {
			setTimeout(function(){ exec(window.jQuery, window._) }, 0);
		}
	}(function($, _){
		"use strict";

		if (!$ || !_) {
			return;
		}

		var $allPanels = $('.js-top-dropdown');

		var load = function($container, callback){
			if ($container.data('loaded')) {
				_.defer(_.bind(callback, $container));
				return;
			}
			$container.load($container.data('url'), function(){
				$container.data('loaded', true);
				callback.call($container);
			});
		};

		var show = function($panel, $allPanels, contentSelector){
			hide($allPanels.not($panel));
			load($panel.find(contentSelector), function(){
				$panel.show();
				$panel.data('state', 'opened');
				$panel.on('click.dropdowns', function(e){
					if (!$(e.target).hasClass('js-top-dropdown-trigger')) {
						e.stopPropagation();
					}
				});
			});
		};

		var hide = function($panel){
			$panel.hide();
			$panel.data('state', 'closed');
			$panel.off('.dropdowns');
		};


		$(document)
			.on('click.dropdowns', function(){
				$allPanels
					.filter(function(){ return 'opened' === $(this).data('state') })
					.each(function(){ hide($(this)) });
			})
			.on('click.dropdowns', '.js-top-dropdown-trigger', function(e){
				e.preventDefault();
				e.stopPropagation();
				var $trigger = $(this);
				var $panels = $($trigger.data('dropdownGroup'));
				var contentSelector = $trigger.data('contentSelector');
				var $panel = $panels.filter(function(){ return $trigger.data('dropdownTarget') === $(this).data('dropdown') });
				'opened' === $panel.data('state') ? hide($panel) : show($panel, $panels, contentSelector);
			});
	}));
</script>

