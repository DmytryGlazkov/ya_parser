<script>
	(function(exec){
		if ('function' === typeof require) {
			require([ 'jquery', 'underscore', 'bloodhound', 'typeahead' ], exec);
		} else {
			setTimeout(function(){ exec(window.jQuery, window._, window.Bloodhound) }, 0);
		}
	}(function($, _, Bloodhound){
		"use strict";

		if (!$ || !_ || !Bloodhound || !$.fn.typeahead/* || $.fn.jquery.replace(/(^|\.)/g, '$100').replace(/.*?(\d{3})(\.|$)/g, '$1') >> 0 < 1009000*/) {
			return;
		}
		var $this = $('#header');

		var data = {};

		var engine = new Bloodhound({
			datumTokenizer: function(d){ return Bloodhound.tokenizers.whitespace(d.val) },
			queryTokenizer: Bloodhound.tokenizers.whitespace,
			limit: 10,
			remote: {
				url: '/internal/ajax.json?query=%QUERY',
				ajax: {
					data: {
						action: 'getSearchResultsFromPart',
						editionid: "52"					}
				},
				filter: function(data){
					if (!data) return [];
					return _.map(data.suggestions, function(v, k){
						return { value: v, data: data.data[k] };
					});
				},
				replace: function(url, query){
					var custom = $.param(data);
					return url.replace(/%QUERY/, encodeURIComponent(query)) + (custom ? '&' + custom : '');
				}
			}
		});

		engine.initialize();

		$this.find('.js-typeahead-search').typeahead(null, {
			source: engine.ttAdapter()
		}).on('typeahead:selected', function(e, value){
			var $field = $(this);
			switch (value.data) {
				case 2:
				case 1:
					$.post('/internal/ajax.json', { action: 'searchdash', type: 1 });
					_.defer(function(){ $field.closest('form').submit() });
					break;
				default:
					$.post('/internal/ajax.json', { action: 'searchdash', type: 2 });
					_.defer(function(){ window.location.href = value.data });
					break;
			}
		}).on('typeahead:opened', function(){
			var ev = $.Event('keydown');
			ev.keyCode = ev.which = 40;
			$(this).trigger(ev);
			return true;
		}).each(function(){
			var $field = $(this);
			$field.closest('form').on('data:change', function(){
				data = $(this).data('autocompleteData') || {};
				engine.clearRemoteCache();
			}).trigger('data:change');
		});
	}));
</script>
