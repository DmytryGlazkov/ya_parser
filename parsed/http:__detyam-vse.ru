            $(this).find('.caption').stop().fadeTo(500, 0);            
            timeout_id = setTimeout(auto_animate1, 5000);            
        });
        
        function auto_animate1()
        {
            slider_animate1('right');            
        }
        
        $(e).find('.FlexNext').click(function(){
            if(!in_progress)
