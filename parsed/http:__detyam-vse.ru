                slider_animate1('right');
            }
            
            return false;
        });
        
        $(e).find('.FlexPrev').click(function(){
            if(!in_progress)
            {
                slider_animate1('left');
            }
            
            return false;
        });
        
        timeout_id = setTimeout(auto_animate1, 5000);        
      
        return true;
    };
})(jQuery);
