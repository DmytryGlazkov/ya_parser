<script type="text/javascript">
    var workspace;
    $(document).ready(function() {
        workspace = Planeta.init({
            myProfile: workspaceInitParameters.myProfile,
            appModel: {
                constructor: AppModel,
                profileConstructor: BaseProfileModel,
                profile: null
            },
            configuration: workspaceInitParameters.configuration,
            routing: {
                navigationMap: null,
                router: SimpleRouter
            },
            isAuthorized: false
        });

        LazyHeader.init({
            auth: headerInitParameters.auth,
            search: {
                searchType: 'products'
            }
        });

        if (true) {
            var shoppingCart = new Shop.Models.ShoppingCart(workspace.appModel.get("myProfile").toJSON());
            var view = new Shop.Views.ShoppingCartWidget({model: shoppingCart});

            workspace.shoppingCart = shoppingCart;
            workspace.shoppingCart.load();
        }
    });

</script>
    

    
    
    

    <script id="profile-display-name" type="text/x-jquery-tmpl">
        <a href="/{{= ProfileUtils.getUserLink(profileId, alias)}}" class="profile-link" profile-link-data="{{= profileId}}">{{= displayName}}</a>
    </script>

    <script id="profile-author-display-name" type="text/x-jquery-tmpl">
        <a itemprop="url" href="{{= ProfileUtils.getAbsoluteUserLink(authorProfileId, authorAlias)}}" class="profile-link" profile-link-data="{{= authorProfileId}}"><span itemprop="name">{{= authorName}}</span></a>
    </script>

    <script id="profile-dialog-display-name" type="text/x-jquery-tmpl">
        <a href="/{{= ProfileUtils.getUserLink(profileId, alias)}}"
           class="profile-link"
           profile-link-data="{{= profileId}}">
            {{html ProfileUtils.getFriendName($data)}}
        </a>
    </script>

    <script id="blog-post-votes-no-tooltips" type="text/x-jquery-tmpl">
        <div class="blog-reit js-blog-rating">
            <div class="cf-count js-blog-post-cf-count">
                {{tmpl "#post-blog-voters-loader-template"}}
                <div class="cf-count-value show-voters">{{= rating}}</div>
            </div>
            <div class="cf-likes-block">
                <div class="cf-like up{{if myVote}}{{if myVote>=1}} active{{/if}}{{/if}}"><i class="icon-com-like"></i></div>
                <div class="cf-dislike down{{if myVote}}{{if myVote<=-1}} active{{/if}}{{/if}}"><i class="icon-com-dislike"></i></div>
            </div>
        </div>
    </script>

    <script id="blog-post-votes" type="text/x-jquery-tmpl">
        <div class="blog-reit js-blog-rating">
            <div class="cf-count js-show-voters">
                {{tmpl "#post-blog-voters-loader-template"}}
                <div class="cf-count-value show-voters">{{= rating}}</div>
            </div>
            <div class="cf-likes-block disabled tooltip" data-original-title="Зайдите в пост, чтобы проголосовать">
                <div class="cf-like up"><i class="icon-com-like"></i></div>
                <div class="cf-dislike down"><i class="icon-com-dislike"></i></div>
            </div>
        </div>
    </script>

    <script id="blog-post-comment-votes" type="text/x-jquery-tmpl">
        <div class="blog-reit no-layout-blog-comment-rating">
            <div class="cf-count">
                {{tmpl "#post-blog-voters-loader-template"}}
                <div class="cf-count-value stat count">{{= rating}}</div>
            </div>
            <div class="cf-likes-block">
                <div class="cf-like up{{if myVote}}{{if myVote > 0}} active{{/if}}{{/if}}"><i class="icon-com-like"></i></div>
                <div class="cf-dislike down{{if myVote}}{{if myVote < 0}} active{{/if}}{{/if}}"><i class="icon-com-dislike"></i></div>
            </div>
        </div>
    </script>

    <script id="profile-small-avatar" type="text/x-jquery-tmpl">
        <a href="/{{= ProfileUtils.getUserLink(profileId, alias)}}">
            {{if profileType == "USER"}}
            <img src="{{= ImageUtils.getUserAvatarUrl(smallImageUrl, ImageUtils.SMALL_AVATAR, userGender)}}"
                 alt="{{= displayName}}"
                 class="profile-link"
                 profile-link-data="{{= profileId}}" />
            {{else}}
            <img src="{{= ImageUtils.getThumbnailUrl(smallImageUrl, ImageUtils.SMALL_AVATAR, ImageType.GROUP)}}"
                 alt="{{= displayName}}"
                 class="profile-link"
                 profile-link-data="{{= profileId}}" />
            {{/if}}
        </a>
    </script>

    <script id="profile-author-small-avatar" type="text/x-jquery-tmpl">
