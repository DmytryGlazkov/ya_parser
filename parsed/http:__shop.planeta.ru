            <img src="{{= ImageUtils.getUserAvatarUrl(authorImageUrl, ImageUtils.SMALL_AVATAR, authorGender) }}" alt="{{= authorName}}" class="profile-link" profile-link-data="{{= authorProfileId}}"/>
        </a>
    </script>

    
    
    

