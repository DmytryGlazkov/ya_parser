    <script id="profile-sidebar-members-array-template" type="text/x-jquery-tmpl">
        {{if typeof(list) != 'undefined' && list != null && list.length }}
        <div class="hideable">
            <p class="sidebar-h{{if className}} {{= className}}{{/if}}">
                {{if profilesLink}}<a href="{{= profilesLink}}" class="sidebar-h-link">Все</a>{{/if}}
                {{= title}} {{= StringUtils.declOfNumWithNumAndBracket(number)}}
            </p>
            <ul class="members">
                {{each list}}
                <li class="item">
                    <a href="/{{= ProfileUtils.getUserLink($value.subjectProfileId, $value.alias)}}">
                        {{if $value.profileType == "USER"}}
                        <img src="{{= ImageUtils.getUserAvatarUrl($value.smallImageUrl, ImageUtils.SMALL_AVATAR, $value.userGender)}}"
                             class="profile-link" profile-link-data="{{= $value.subjectProfileId}}"/>
                        {{else $value.profileType == "GROUP"}}
                        <img src="{{= ImageUtils.getThumbnailUrl($value.smallImageUrl, ImageUtils.SMALL_AVATAR, ImageType.GROUP)}}"
                             class="profile-link" profile-link-data="{{= $value.subjectProfileId}}"/>
                        {{else $value.profileType == "EVENT"}}
                        <img src="{{= ImageUtils.getThumbnailUrl($value.smallImageUrl, ImageUtils.SMALL_AVATAR, ImageType.EVENT)}}"
                             class="profile-link" profile-link-data="{{= $value.subjectProfileId}}"/>
                        {{/if}}
                    </a>
                </li>
                {{/each}}
            </ul>
        </div>
        {{/if}}
    </script>

