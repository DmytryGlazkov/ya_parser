<script type="text/javascript">
$(document).ready(function() {


	 var defaultDiscountIn = "Введите номер вашей карты";
	 
	 $('#dicountcardfooter').val(defaultDiscountIn);
	 
	  $('#dicountcardfooter').focus(function() {
	    if($(this).val() == defaultDiscountIn) $(this).val("");
	  });
	
	  $('#dicountcardfooter').focusout(function() {
	    if($(this).val() == "") $(this).val(defaultDiscountIn);
	  });

var validator = new Barcoder('ean13');



$("#dicountcardfooter + a").fancybox({
		'type'			:	'iframe',
		'width'			:	500,
		'transitionIn'	:	'elastic',
		'transitionOut'	:	'elastic',
		'speedIn'		:	600, 
		'speedOut'		:	200, 
		'overlayShow'	:	true,
		'hideOnOverlayClick' : true,
		'height' : 200,
		'autoSize' : false
});


$('#dicountcardfooter').bind('change blur keyup mouseup', function() {

if(validator.validate($(this).val()) == true){
	$('#dicountcardfooter + a').removeClass("disc-inactive");
	$('#dicountcardfooter + a').addClass("disc-active");
	$("#dicountcardfooter + a").attr({
		href: "/modal/?card=" + $(this).val(),
	});

}
else {
	$("#dicountcardfooter + a").addClass("disc-inactive");
	$("#dicountcardfooter + a").attr({
		href: "/modal/?card=1",
	});
}


});


});

</script>


