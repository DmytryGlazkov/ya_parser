				<script>
function SetCookie(cookieName,cookieValue,nDays, domainName) {
	var today = new Date();
	var expire = new Date();
	if (cookieName == 'GeoCookie') {
		//������� ����� ���� ������ ��� ������ mts.ru
		domainName = 'mts.ru';
	} else if (typeof domainName == 'undefined') {
		var domainName = "shop.mts.ru";			
	}
	if (nDays==null || nDays==0) nDays=1;
	expire.setTime(today.getTime() + 3600000*24*nDays);
	document.cookie = cookieName+"="+escape(cookieValue)+ ";expires="+expire.toGMTString()+";path=/; domain=."+domainName;
}

