                    fitToView   : false,
                    scrollOutside   : true,
                    openEffect	: 'elastic',
                    closeEffect	: 'elastic',
                    nextEffect 	: 'elastic',
                    prevEffect	: 'elastic',
                    tpl:{
                        closeBtn : '<a class="fancybox-item magazine_table_close" href="javascript:;"></a>'
                    }
                });

                $('.gallery-thumb_img').fancybox({
                    openEffect	: 'elastic',
                    closeEffect	: 'elastic',
                    nextEffect 	: 'none',
                    prevEffect	: 'none',
                    wrapCSS     : 'fancy_gallery',
                    helpers	: {
                        thumbs	: {
                            width	: 50,
                            height	: 50
                        }
                    }
                });

                $('.main_image-zoom').on("click", function(){
                    var index = $('.gallery .active').index(),
                        img = $(".gallery-thumb_img");
                    if(index >= 0){
                        img.eq(index).trigger("click");
                    }else{
                        img.eq(1).trigger("click");
                    }

                });



                $('.co-picture').each(function(){
                    $(this).find('a:first-child').addClass('co-picture-active');
                });


                $('.b-sorting__select').each(function(){
                    var title = $(this).attr('title');
                    if( $('option:selected', this).val() != ''  ) title = $('option:selected',this).text();
                    $(this)
                            .after('<span class="b-sorting__select-title">' + title + '</span>')
                            .change(function(){
                                val = $('option:selected',this).text();
                                $(this).next().text(val);
                            })
                });
            });

            $(document).mouseup(function (e)
            {
                var container = $(".popup_cart_wrap");

                if (container.has(e.target).length === 0)
                {
                    container.hide();
                }
            });
        </script>


