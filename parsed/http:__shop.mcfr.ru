                    <script type="text/javascript">
    $(function(){
        $(".phonecall_form_link").colorbox({inline:true, href:".phonecall_form", width: '750px'});
        var formId = 'form_phonecall';

        $("#" + formId).submit(function() {
            var url = '/index/phonecallvalidation/';
            var data = {};
            $("#" + formId +" input").each(function(){
                data[$(this).attr('name')] = $(this).val();
            });
            $("#" + formId +" textarea").each(function(){
                data[$(this).attr('name')] = $(this).val();
            });
            $("#" + formId +" select").each(function(){
                data[$(this).attr('name')] = $(this).val();
            });

            $.post(url, data, function(resp) {
                if (resp['errors'] == null) {
                    $.colorbox({html:'<h2>Спасибо, ваша заявка принята, менеджер вскоре свяжется с вами!</h2><a href="#" onclick="$.colorbox.close(); return false;">Закрыть</a>'});
                    return true;
                }

                $("#" + formId).find('.errors').remove();
                for (elementName in resp['errors']) {
                    for (i in resp['errors'][elementName]) {
                        $("#" + formId + " #" + elementName + "-element").append('<ul class="errors"><li>' + resp['errors'][elementName][i] + '</li></ul>');
                    }
                }
                $.colorbox.resize();
            },"json");
            return false;
        });
    });
</script>

