            $("#dosub").hover (
                function() {
                    $(this).addClass("btnhover");
                },
                function() {
                    $(this).removeClass("btnhover");
                }
            );

            $('#do_login').click(function() {
                var goto = '/';
                var btn = $('#do_login');
                btn.hide().next().show();
                $.post( "/utils/do_login/", {
                    login: $('#login').val(),
                    password: $('#loginning_password').val(),
                    _goto: goto
                }, function(data){
                    if (data.errors == null){
                        btn.next().hide().prev().show();
                        loginForm.jqmHide();
                        window.location.href = goto;
                        return false;
                    } else {
                        if(data.errors.login_errors){
                            for (er in data.errors.login_errors){
                                $('#tdlogininfo').html(data.errors.login_errors[er]);
                                btn.next().hide().prev().show();
                            };
                        } else {
                            $('#tdlogininfo').html('');
                        };
                        if(data.errors.password_errors){
                            for (er in data.errors.password_errors){
                                $('#tdlogpasswordinfo').html(data.errors.password_errors[er]);
                                btn.next().hide().prev().show();
                            };
                        } else {
                            $('#tdlogpasswordinfo').html('');
                        };
                    }
                },
                "json");
                return false;
            });// end of click

        });
