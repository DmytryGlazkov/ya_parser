        (function($) {
            $.fn.dropdownMenu = function(options) {
                var options = jQuery.extend({
                    "shift": 1 // смещение выпадающего блока по оси х
                },options);
                if (typeof options["wrapper"] == "undefined") {
                    options["wrapper"] = $(this);
                };
                var menuWrapper = options["wrapper"];
                return this.each(function() {
                    var openedSubmenu = null;
                    var menu = $(this);
                    var maxX;
                    $(this).children().not(".not-dropdown").hover(
                        function() {
                            var menuWidth = menuWrapper.width();
                            var minX = menuWrapper.offset().left;
                            var maxX = minX + menuWidth;
                            var submenu = $(this).find("div").filter(".b-dropdown-menu");
                            if (submenu.length != 0) {
                                openedSubmenu = submenu;
                                submenu.css("visibility", "hidden").show();
                                var offset = $(this).height();
                                var coords = $(this).offset();
                                var posX = coords.left;
                                if (!$(this).children("a").hasClass("b-dropdown-menu-first")) {
                                    posX = posX - options.shift;
                                };
                                var posY = coords.top + offset;
                                // значение left, если ширина меню больше доступной области и его нужно отрисовать с правого угла
                                var submenuWidth = submenu.width();
