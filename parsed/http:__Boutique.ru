<script type="text/javascript">
    /* <![CDATA[ */
    var do_not_switch   = false;
    timeout_handler = null;
    var startSh = true;
    var currentShowcase;
    var countShowcase;

    function show_next_showcase ( index, interval )
    {
        if ( do_not_switch ) {
            timeout_handler = setTimeout ( function () { show_next_showcase ( index, interval ) }, interval );
        }
        else
        {
            if ( $("div.showcase").length == 1 )
            {
                $("div.showcase").fadeIn ();
                return;
            }
            clearTimeout ( timeout_handler );
            var next = index == $(".showcase").length - 1 ? 0 : index + 1;
            currentShowcase = index;
            if (!startSh) {
                $("div.showcase").fadeOut ();
            } else {
            	startSh = false;
            };
            $($("div.showcase")[index]).fadeIn ();
            timeout_handler = setTimeout ( function () { show_next_showcase ( next, interval ) }, interval );
        }
    }

    $(function(){

        $("#showcases_box").find('div.showcase').eq(0).show();
        var flagmodal = false;
        var countShowcase = 5 - 1;

        if ($.browser.msie) {
            $("#contentflash").hover(
                    function () {
                        $(this).find(".showcase-btn").show();
                        do_not_switch = true;
                    },
                    function () {
                        $(this).find(".showcase-btn").hide();
                        do_not_switch = false;
                    }
                );//end of hover
        } else {
            $("#contentflash").hover(
                    function () {
                        $(this).find(".showcase-btn").fadeIn();
                        do_not_switch = true;
                    },
                    function () {
                        $(this).find(".showcase-btn").fadeOut();
                        do_not_switch = false;
                    }
                );//end of hover
        };

        $("#showcase-prev, #showcase-next").hover(
            function () {
                $(this).addClass("btnhover");
            },
            function () {
                $(this).removeClass("btnhover");
            }
        );//end of hover

        $("#showcase-prev").click( function() {
            do_not_switch = false;
            if (currentShowcase==0) {
                currentShowcase = countShowcase;
            } else {
                currentShowcase -=1;
            };
            show_next_showcase ( currentShowcase, 10000 );
        });//end of click

        $("#showcase-next").click( function() {
            do_not_switch = false;
            if (currentShowcase==countShowcase) {
                currentShowcase = 0;

            } else {
                currentShowcase +=1;
            };
            show_next_showcase ( currentShowcase, 10000 );
        });//end of click

        $("#showcases_box").mouseover ( function () {
            do_not_switch = true;
        });

        $("#showcases_box").mouseout ( function () {
            do_not_switch = false;
        });


        $(".showcase").each ( function () {
            var that = this;

            $("div.area", this).each ( function () { // IE7 fix
                $(this).css ( "filter", "alpha(opacity=0)" );
            });

            $("div.area", this).hover ( function () {
                $("div.area", that).each ( function () {
                    $(this).stop ().animate ( {opacity: 0.4}, 350 );
                });

                $(this).stop ().animate ( {opacity: 0}, 350 );
            });

            $(this).hover ( function () {}, function () {
                $("div.area", this).each ( function () {
                    $(this).stop ().animate ( {opacity: 0}, 350 );
                });
            });
        });

        show_next_showcase(0, 10000);

        // отправка статистики по витринам в ГА
        $(document).on('click', '.showcase a, .showcase area', function() {
            var url = this.href;
