            });


            var regForm = $('#registration_fly');
            var loginForm = $('#logup_fly');
            var buyInfo = $('#buyfromusdiv');

            regForm
            .add(loginForm)
            .keypress( function(e){
                if (e.keyCode == 13) {
                    $(this).find('td.popup-btns a').eq(0).click();
                }
            })
            .add(buyInfo)
            .jqm({
                trigger: false,
                overlay: 20,
                modal: false
