                                    // хак баги в IE 7 - блочные элементы не растягиваются на всю длину родителя, если для него не задана ширина
                                    submenuWidth += 1;
                                    submenu.width(submenuWidth);
                                    var colWidth = submenu.children().find("ul:first-child").width();
                                    submenu.find("li").filter(".b-dropdown-menu-separator").width(colWidth - 17);
                                };
                                var rightShift = posX + submenuWidth - maxX;
                                var leftShift = minX - (posX + $(this).width() - submenuWidth);
                                if (submenuWidth > (maxX - posX)) {
                                    var leftShift = minX - (posX + $(this).width() - submenuWidth);
                                    if ((rightShift > 0)&&(leftShift < rightShift)) {
                                        posX = Math.round(posX + $(this).width() - submenuWidth) + options.shift*3;
                                        if ($(this).children("a").hasClass("b-dropdown-menu-last")) {
                                            posX = Math.round(maxX - submenuWidth);
                                        };
                                    };
                                };
                                submenu.css({"left": posX, "top" : posY, "visibility": "visible"});
                            };
                        },
                        function() {
                            if (openedSubmenu != null) {
                                openedSubmenu.hide();
                            };
                        }
                    );
