                $('.refresh_subscription_captcha').click(function() {
                    $.getJSON('/captcha/refresh/', function(data) {
                        $('#subscription_captcha_key').val(data.key);
                        $('#subscription_captcha_image').attr('src', data.image_url);
                    });
                    return false;
                });
                subscribeBtn.click( function(gender) {
                    // 1 is male, 2 is female, 0 is undefined gender
                    if ( gender != 1 && gender != 2 )
                        gender = 0;
                    var _result = $('<div>');
                    $('#subscription_result').append(_result);
                    _result.load('/utils/subscribe/', {
                        email: $('#subscription_email').val(),
                        captcha_key: $('#subscription_captcha_key').val(),
                        captcha: $('#subscription_captcha').val(),
                        section: $('#subscription_section_id').val(),
                        category: $('#subscription_category_id').val(),
                        brand: $('#subscription_brand_id').val(),
                        gender: gender
                    });
                    return false;
                });

                var userInfoTable = $('#reg-user-table');
                $("#register_button").click(function(e) {
                    e.preventDefault();
                    showRegisterForm();
                });

                var registrationForm = $("#registration_fly");

                var infoRegInputs = $("td").filter('.td-error-i');
                var doRegBtn = $('#goon');
                var spinner = $('#goon').next();

                $('.refresh_captcha').click(function() {
                    $.getJSON('/captcha/refresh/', function(data) {
                        $('#reg_captcha_key').val(data.key);
                        $('#reg_captcha_image').attr('src', data.image_url);
                    });
                    return false;
                });

                doRegBtn.click(function() {
                    $('#tdrpasswordinfo').parent().addClass("el-hidden")
                    if ($("#password").val() == $("#rpassword").val()) {
                        doRegBtn.hide();
                        spinner.show();
                        infoRegInputs.text('');
                        var phones = getPhones();
                        if (!phones) {
                            doRegBtn.show();
                            spinner.hide();
                            return false;
                        };
                        $('#signup_result').load('/utils/check_registration/', {
                            name: $('#name').val(),
                            surname: $('#surname').val(),
                            phones: phones,
                            email: $('#email').val(),
                            password: $('#password').val(),
                            captcha_key: $('#reg_captcha_key').val(),
                            captcha: $('#reg_captcha').val()
                        });
                        return false;
                    } else {
                        $('#tdrpasswordinfo').text('пароли не совпадают').parent().removeClass("el-hidden");
                    };
                });// end of click
            });
        /* ]]> */
        </script>

