			}
		},
		function(){
			if(!$(this).parent().hasClass('active'))
			{
				$(this).css({backgroundColor:'white'});
				$(this).find('div.tab_head_corner').css({backgroundPosition:'left 0'});
			}
		}
	);
	
	$('span.submenu_triangle').each(function(){
		$(this).css({
			marginLeft:($(this).parent().width() - 9),
			top:($('#catalog_menu_vertical').position()['top'] + 26)
		}).show();
	});
	
	$('#catalog_menu_vertical td').hover(
		function(){
			if(!$(this).hasClass('delimiter'))
				$(this)
					.css({backgroundColor:'#b2b297'})
					.find('a.parent_link')
					.css({color:'white'});
				$(this)
					.find('span.submenu_triangle')
					.css({
						borderTop:'8px solid #ffd94c',
						borderLeft:'4px solid #b2b297',
						borderRight:'4px solid #b2b297'
					});
				$(this)
					.find('ul')
					.css('top', $(this).position()['top'] + $(this).height())
					.show();
				$(this)
					.find('a.child_link')
					.css({color:'white'});
		},
