<script type="text/javascript">

var step = 0;
var ifFirst = true;
    
function moveArrow()
{	
	if(step == 0)
	{
		$('#arrow').animate({top: '+=75px'}, 1500, function(){
			if(ifFirst)
			{
				step++;
				ifFirst = false;
			}
			else
			{
				$('.baner_items td').removeClass('selected');
				$('#link' + step).addClass('selected');
			
				$('#banner1_img').fadeOut('fast',function(){
					$('#banner1_img img').attr('src', $('#img' + step).attr('src'));
					$('#banner1_img').attr('href', $('#link' + step).attr('href'));
					$('#banner1_img').fadeIn('fast');
					
					step++;
				});
			}
		});
	}
	else if(step < 4)
	{
		$('#arrow').animate({top: '+=75px'}, 1500, function(){
			$('.baner_items td').removeClass('selected');
			$('#link' + step).addClass('selected');
			
			$('#banner1_img').fadeOut('fast',function(){
				$('#banner1_img img').attr('src', $('#img' + step).attr('src'));
				$('#banner1_img').attr('href', $('#link' + step).attr('href'));
				$('#banner1_img').fadeIn('fast');
				
				step++;
			});
		});
	}
	else
	{
		$('#arrow').animate({top: '+=75px'}, 1500, function(){
			$('#arrow').css('top', '-53px');
			
			step = 0;
		});
	}
}

$(document).ready(function(){
	moveArrow();
	window.setInterval('moveArrow()', 7000);
});

</script>

