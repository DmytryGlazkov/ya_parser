				if($(this).attr('id') == 'header_tab2')
				{
					$(this).find('div.tab_head').width(55);
					$(this).find('div.tab_head_corner').css('marginLeft', 55);
					
					$(this).find('div.tab_head2_content1').show();
					$(this).find('div.tab_head2_content2').hide();
				}
				
				$(this).removeClass('active').addClass('inactive');
			});
			
			$('#header_right div.tab_head3').css('marginLeft', 155);
			
			if($(this).hasClass('tab_head2'))
			{
				$(this).find('div.tab_head2_content1').hide();
				
				$(this).width(200).find('div.tab_head2_content2').show();
				$(this).find('div.tab_head_corner').css('marginLeft', 200);
				
				$('#header_right div.tab_head3').css('marginLeft', 295);
			}
			
			$(this).parent().removeClass('inactive').addClass('active');
			$(this).css({backgroundColor:'white'});
			$(this).find('div.tab_head_corner').css({backgroundPosition:'left 0'});
		}
	);
	
	$('#header_right div.tab_head')
