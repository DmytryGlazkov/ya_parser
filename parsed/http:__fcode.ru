       function setCookie(value){
          var name='actionpay';
          var d = new Date();
          expires=d.getTime() + (1000 * 86400 * 14);
          var exp = new Date(expires);
          document.cookie = name + '=' + value + '; expires=' + exp +  '; path=/';
      }

      var tmp = new Array(); 
      var param = new Array();

      var get = location.search; 
