	<script>
        function get_test_segment(test_name,segments){
            var segment_name;
            //checking segment
            segment_name=jQuery.cookie(test_name);
            if(segment_name){
                return segment_name;
            }
            //generating_segment
            var sum=0;
            var segment_P;
            for(segment_name in segments){
                segment_P=segments[segment_name];
                sum+=segment_P;
            }
            var random=sum*Math.random();
            sum=0;
            for(segment_name in segments){
                segment_P=segments[segment_name];
                if(random>=sum && random<(sum+segment_P)){
                    jQuery.cookie(test_name, segment_name, { path: '/', expires: 30, domain: '.detishop.ru' });
                    return segment_name;
                }
                sum+=segment_P;
            }
            return null;
        }
