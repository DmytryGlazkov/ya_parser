<h2>Города присутствия</h2>
<p>
<div id="map" style="width:100%; height:400px"></div>

<script src="http://api-maps.yandex.ru/2.0/?load=package.full&lang=ru-RU" type="text/javascript"></script>
<script type="text/javascript">
function init () {
    var myMap = new ymaps.Map("map", {
            center: [55.77, 37.64],
            zoom: 10
        });
    myMap.controls
        .add('zoomControl')
        .add('mapTools')
        .add('typeSelector');

    var myCollection = new ymaps.GeoObjectCollection();

    //pvz.forEach(function(item){
    $.each(pvz, function(index, item){
        var latitude = item.latitude;
        var longitude = item.longitude;
        var description = item.name;
        var balloon = '<strong>' + item.name + '</strong>';
        if (item.address != undefined && item.address.length) balloon = balloon + '<br /><i>' + item.address + '</i>';
        if (item.work_time != undefined && item.work_time.length) balloon = balloon + '<br /><i>' + item.work_time + '</i>';
        if (item.howtogo != undefined && item.howtogo.length) balloon = balloon + '<hr />' + item.howtogo;
        if (item.payment_type != undefined && item.payment_type.length) balloon = balloon + '<br /><br />' + 'Оплата: ' + item.payment_type;
        
        myCollection.add(new ymaps.Placemark([latitude, longitude], {balloonContent: balloon, hintContent: description}));
    });
    
    myMap.geoObjects.add(myCollection);
    myMap.setBounds(myMap.geoObjects.getBounds());
    var zoom = myMap.getZoom();
    if (pvz.length == 1)
       zoom = 12;
